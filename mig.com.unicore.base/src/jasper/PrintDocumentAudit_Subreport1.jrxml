	<parameter name="UNS_AuditPartner_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT org.Value AS Org, bpl.Name AS Location, COALESCE(ry.Name, 'UNDEFINED') AS Rayon, iv.DocumentNo AS Invoice, iv.DateInvoiced AS DateInvoiced, dt.Name AS DocType, COALESCE(usr.RealName, usr.Name) AS Sales, iv.GrandTotal AS GrandTotal, InvoiceOpen(iv.C_Invoice_ID, 0) AS OpenAmt FROM UNS_AuditDocument ad

INNER JOIN C_Invoice iv ON iv.C_Invoice_ID = ad.C_Invoice_ID
INNER JOIN C_DocType dt ON dt.C_DocType_ID = iv.C_DocType_ID
INNER JOIN AD_Org org ON org.AD_Org_ID = iv.AD_Org_ID
INNER JOIN AD_User usr ON usr.AD_User_ID = iv.SalesRep_ID
INNER JOIN C_BPartner_Location bpl ON bpl.C_BPartner_Location_ID = iv.C_BPartner_Location_ID
LEFT JOIN UNS_Rayon ry ON ry.UNS_Rayon_ID = bpl.UNS_Rayon_ID

WHERE ad.UNS_AuditPartner_ID = $P{UNS_AuditPartner_ID}

ORDER BY iv.DateInvoiced ASC, iv.DocumentNo ASC]]>
	</queryString>

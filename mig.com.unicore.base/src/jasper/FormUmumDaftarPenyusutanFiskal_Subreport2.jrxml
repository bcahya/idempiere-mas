		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT COALESCE(SUM(Exp.Expense),0) AS komersial,
COALESCE(SUM(Exp.Expense_F),0) AS fiskal

FROM A_Depreciation_Exp Exp
INNER JOIN A_Asset A ON A.A_Asset_ID=Exp.A_Asset_ID
INNER JOIN A_Depreciation_Workfile DW ON DW.A_Asset_ID=A.A_Asset_ID

WHERE A.A_Asset_ID=$P{A_Asset_ID} AND EXTRACT (YEAR FROM $P{EndDate} ::Timestamp) = EXTRACT (YEAR FROM Exp.DateAcct) AND Exp.DateAcct <= $P{EndDate} ::Timestamp AND Exp.Processed='Y'
]]>
	</queryString>
	<field name="komersial" class="java.math.BigDecimal"/>
	<field name="fiskal" class="java.math.BigDecimal"/>

	<queryString>
		<![CDATA[SELECT COALESCE (SUM(DW.A_Accumulated_Depr),0) AS accumDepr,
COALESCE(SUM(DW.A_Accumulated_Depr_F),0) AS accumDeprF,
COALESCE(SUM(Exp.Expense),0) AS komersial,
COALESCE(SUM(Exp.Expense_F),0) AS fiskal

FROM A_Depreciation_Exp Exp
INNER JOIN A_Asset A ON A.A_Asset_ID=Exp.A_Asset_ID
INNER JOIN A_Depreciation_Workfile DW ON DW.A_Asset_ID=A.A_Asset_ID

WHERE EXTRACT (YEAR FROM $P{EndDate} ::Timestamp) = EXTRACT (YEAR FROM Exp.DateAcct) AND Exp.DateAcct <= $P{EndDate} ::Timestamp

GROUP BY DW.A_Accumulated_Depr, DW.A_Accumulated_Depr_F]]>
	</queryString>
	<field name="accumdepr" class="java.math.BigDecimal"/>
	<field name="accumdeprf" class="java.math.BigDecimal"/>
	<field name="komersial" class="java.math.BigDecimal"/>
	<field name="fiskal" class="java.math.BigDecimal"/>
	<variable name="komersial_1" class="java.math.BigDecimal" resetType="Column" calculation="Sum">
		<variableExpression><![CDATA[$F{accumdepr}-$F{komersial}]]></variableExpression>
	</variable>
	<variable name="fiskal_1" class="java.math.BigDecimal" resetType="Column" calculation="Sum">
		<variableExpression><![CDATA[$F{accumdeprf}-$F{fiskal}]]></variableExpression>
	</variable>

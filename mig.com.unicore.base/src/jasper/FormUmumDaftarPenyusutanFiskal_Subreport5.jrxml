<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="FormUmumDaftarPenyusutanFiskal_Subreport5" language="groovy" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="d55da63e-8a2c-405f-92b8-9a9d7a388462">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="EndDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="AD_Org_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="A_Asset_Group_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT COALESCE (SUM(DW.A_Accumulated_Depr),0) AS accumDepr,
COALESCE(SUM(DW.A_Accumulated_Depr_F),0) AS accumDeprF,
COALESCE(SUM(Exp.Expense),0) AS komersial,
COALESCE(SUM(Exp.Expense_F),0) AS fiskal

FROM A_Depreciation_Exp Exp
INNER JOIN A_Asset A ON A.A_Asset_ID=Exp.A_Asset_ID
INNER JOIN A_Depreciation_Workfile DW ON DW.A_Asset_ID=A.A_Asset_ID

WHERE EXTRACT (YEAR FROM $P{EndDate} ::Timestamp) = EXTRACT (YEAR FROM Exp.DateAcct) AND Exp.DateAcct <= $P{EndDate} ::Timestamp AND Exp.Processed='Y' AND A.AD_Org_ID=$P{AD_Org_ID} AND A.A_Asset_Group_ID=$P{A_Asset_Group_ID}

GROUP BY DW.A_Accumulated_Depr, DW.A_Accumulated_Depr_F]]>
	</queryString>
	<field name="accumdepr" class="java.math.BigDecimal"/>
	<field name="accumdeprf" class="java.math.BigDecimal"/>
	<field name="komersial" class="java.math.BigDecimal"/>
	<field name="fiskal" class="java.math.BigDecimal"/>
	<variable name="komersial" class="java.math.BigDecimal" resetType="None">
		<variableExpression><![CDATA[$F{accumdepr}==0 ? $F{accumdepr} : $F{accumdepr}-$F{komersial}]]></variableExpression>
	</variable>
	<variable name="fiskal" class="java.math.BigDecimal" resetType="None">
		<variableExpression><![CDATA[$F{accumdeprf}==0 ? $F{accumdeprf} : $F{accumdeprf}-$F{fiskal}]]></variableExpression>
	</variable>
	<background>
		<band splitType="Stretch"/>
	</background>

	<queryString>
		<![CDATA[select now() as tglsekarang,mc.documentno Documentno,mv.movementdate as tglmove,mv.documentno  as mdocumentno,mc.receiptdate as tglkonfirm,mc.description as description,wh.name as gudang,ad.name as user
 from M_MovementConfirm mc
 left join  M_Movement mv on mv.m_movement_id=mc.m_movement_id
 inner join m_warehouse wh on wh.m_warehouse_id=mv.destinationwarehouse_id
 left join ad_user ad on ad.ad_user_id=$P{AD_User_ID}
 where mc.docstatus in('CO','CL') AND mc.M_MovementConfirm_ID=$P{M_MovementConfirm_ID}]]>
	</queryString>

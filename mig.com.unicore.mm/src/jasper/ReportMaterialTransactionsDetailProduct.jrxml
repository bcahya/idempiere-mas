	<queryString>
		<![CDATA[SELECT COALESCE(mi.documentno,mt.documentno,pc.documentno,pl.documentno,mm.documentno, pn.documentno) AS DocumentNo, wh.Name AS Warehouse, COALESCE(ads.RealName, ads.name) AS Created, loc.Value AS Locator, p.Name AS Items, p.value AS DocumentProduct, rl.Name AS Type, trx.MovementQty AS Qty, trx.MovementDate AS MovementDate,now() AS DatePrint, COALESCE(uom.UOMSymbol, uom.Name) AS UOM, p.M_Product_ID AS IDPRoduct, loc.M_Locator_ID AS IDLoc, CASE WHEN $P{DateTo}::timestamp IS NOT NULL THEN $P{DateTo}::timestamp ELSE now()::timestamp END AS DateTrx,
(SELECT COALESCE(SUM(QtyOnHand),0) FROM M_Storage s WHERE s.M_Product_ID = p.M_Product_ID AND s.M_Locator_ID = loc.M_Locator_ID AND QtyOnHand <> 0) AS StockNow

FROM M_Transaction trx

INNER JOIN M_Product p ON p.M_Product_ID = trx.M_Product_ID
INNER JOIN M_Product_Category mc ON mc.M_Product_Category_ID=p.M_Product_Category_ID
INNER JOIN C_UOM uom ON uom.C_UOM_ID = p.C_UOM_ID
INNER JOIN M_Locator loc ON loc.M_Locator_ID = trx.M_Locator_ID
INNER JOIN M_Warehouse wh ON wh.M_Warehouse_ID = loc.M_Warehouse_ID
INNER JOIN AD_Reference rf ON rf.Name = 'M_Transaction Movement Type'
INNER JOIN AD_Ref_List rl ON rl.AD_Reference_ID = rf.AD_Reference_ID AND rl.Value = trx.MovementType
INNER JOIN AD_Org ao ON ao.AD_Org_ID = wh.AD_Org_ID
LEFT JOIN AD_User us ON us.AD_User_ID = $P{AD_User_ID}
INNER JOIN AD_User ads ON ads.AD_User_ID=trx.Createdby
LEFT JOIN AD_User_OrgAccess uo ON uo.AD_User_ID = us.AD_User_ID
LEFT JOIN M_InventoryLine mil ON mil.M_InventoryLine_ID=trx.M_InventoryLine_ID
LEFT JOIN M_Inventory mi ON mi.M_Inventory_ID=mil.M_Inventory_ID
LEFT JOIN M_InoutLine min ON min.M_InoutLine_ID=trx.M_InoutLine_ID
LEFT JOIN M_Inout mt ON mt.M_Inout_ID=min.M_Inout_ID
LEFT JOIN UNS_PL_Confirmproduct pcp ON pcp.UNS_PL_Confirmproduct_ID=trx.UNS_PL_Confirmproduct_ID
LEFT JOIN UNS_PL_Confirm pc ON pc.UNS_PL_Confirm_ID=pcp.UNS_PL_Confirm_ID
LEFT JOIN UNS_PackingList pl ON pl.UNS_PackingList_ID=pc.UNS_PackingList_ID
LEFT JOIN UNS_Production_detail pd ON pd.UNS_Production_detail_ID=trx.UNS_Production_detail_ID
LEFT JOIN UNS_Production pn ON pn.UNS_Production_ID=pd.UNS_Production_ID
LEFT JOIN M_MovementLine mml ON mml.M_MovementLine_ID=trx.M_MovementLine_ID
LEFT JOIN M_Movement mm ON mm.M_Movement_ID=mml.M_Movement_ID


WHERE (CASE WHEN $P{DateFrom}::timestamp IS NOT NULL AND $P{DateTo}::timestamp IS NOT NULL THEN trx.MovementDate >= $P{DateFrom}::timestamp AND trx.MovementDate <= $P{DateTo}::timestamp
WHEN $P{DateFrom}::timestamp IS NULL AND $P{DateTo}::timestamp IS NOT NULL THEN trx.MovementDate >= $P{DateTo}::timestamp
WHEN $P{DateFrom}::timestamp IS NOT NULL AND $P{DateTo}::timestamp IS NULL THEN trx.MovementDate >= $P{DateFrom}::timestamp ELSE 1=1 END)
AND
(CASE WHEN $P{AD_Org_ID} IS NOT NULL THEN ao.AD_Org_ID = $P{AD_Org_ID} WHEN us.UserAgent = 'N' THEN ao.AD_Org_ID = us.AD_Org_ID ELSE 1=1 END)
AND
(CASE WHEN $P{M_Product_ID} IS NOT NULL THEN p.M_Product_ID = $P{M_Product_ID} ELSE 1=1 END)
AND
(CASE WHEN $P{M_Warehouse_ID} IS NOT NULL THEN wh.M_Warehouse_ID = $P{M_Warehouse_ID} ELSE 1=1 END)
AND
(CASE WHEN $P{M_Product_Category_ID} IS NOT NULL THEN mc.M_Product_Category_ID = $P{M_Product_Category_ID} ELSE 1=1 END)

ORDER BY Items, Warehouse, Locator, MovementDate, Type, created]]>
	</queryString>

	<queryString>
		<![CDATA[select ao.name as FromDepartement, aot.name as DestinationDepartement, mwf.name as FromWareHouse, mw.name as DestinationWareHouse, mm.movementdate as MovementDate, ml.value as Locator, mlt.value as LocatorTo, mp.name as Product, mml.movementqty as MovementQty, mml.confirmedqty as QtyConfirm, mml.scrappedqty as Scrapped, cu.name as UOM

from m_movement mm

inner join ad_org ao ON ao.ad_org_id = mm.ad_org_id

inner join m_warehouse mw ON mw.m_warehouse_id = mm.destinationwarehouse_id

inner join m_movementline mml ON mml.m_movement_id = mm.m_movement_id

inner join m_locator ml ON ml.m_locator_id = mml.m_locator_id

inner join m_warehouse mwf ON mwf.m_warehouse_id = ml.m_warehouse_id

inner join ad_org aot ON aot.ad_org_id = mwf.ad_org_id

inner join m_locator mlt ON mlt.m_locator_id = mml.m_locatorto_id

inner join m_product mp ON mp.m_product_id = mml.m_product_id

inner join c_uom cu ON cu.c_uom_id = mp.c_uom_id

where mm.isintransit = 'Y' AND mm.docstatus = 'CO' AND ao.ad_org_id = $P{AD_Org_ID} AND mm.movementdate between $P{StartDate} AND $P{EndDate}

order by aot.name, mwf.name, mw.name, mm.movementdate, ml.value, mlt.value, mp.name]]>
	</queryString>

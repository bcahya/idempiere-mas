	<parameter name="M_Movement_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT mm.documentno AS docno, mv.documentno as docconf, org.name AS OrgName, kode.value AS kodebarang, kode.Name AS namaBarang, mw.name AS DestWarehouse, mm.movementdate AS mdate,
COALESCE(mm.description, mm.description, '-') AS des, ml.value AS Loc, mlt.value AS Locto,
ROUND(coalesce(mml.UOMmovementqtyL1,'0'),0)
AS Qty1, cu1.name AS UOMConvL1,
ROUND(coalesce(mml.UOMmovementqtyL2,'0'),0)
AS Qty2, cu2.name AS UOMConvL2,
ROUND(coalesce(mml.UOMmovementqtyL3,'0'),0)
AS Qty3, cu3.name AS UOMConvL3,
ROUND(coalesce(mml.UOMmovementqtyL4,'0'),0)
AS Qty4, cu4.name AS UOMConvL4,
uns_shipping."driver" as Supir,
uns_armada."value" as nomobil
FROM M_Movement mm
INNER JOIN M_Warehouse mw ON mw.M_Warehouse_ID = mm.DestinationWarehouse_ID
INNER JOIN ad_org org ON org.AD_Org_ID = mm.AD_Org_ID
INNER JOIN m_movementline mml ON mml.m_movement_id = mm.m_movement_id
INNER JOIN M_Locator ml ON ml.M_Locator_ID = mml.M_Locator_ID
INNER JOIN M_Locator mlt ON mlt.M_Locator_ID = mml.M_Locatorto_ID
INNER JOIN M_Product kode ON kode.M_Product_ID = mml.M_Product_ID
LEFT JOIN C_UOM cu1 ON cu1.C_UOM_ID = kode.UOMConversionL1_ID
LEFT JOIN C_UOM cu2 ON cu2.C_UOM_ID = kode.UOMConversionL2_ID
LEFT JOIN C_UOM cu3 ON cu3.C_UOM_ID = kode.UOMConversionL3_ID
LEFT JOIN C_UOM cu4 ON cu4.C_UOM_ID = kode.UOMConversionL4_ID
LEFT JOIN uns_shippingfreight shf on shf.m_movement_id=mm.m_movement_id
LEFT JOIN uns_shipping on uns_shipping.uns_shipping_id=shf.uns_shipping_id
left Join uns_armada on uns_shipping."uns_armada_id"=uns_armada."uns_armada_id"
left join  M_MovementConfirm mv on mv.m_movement_id=mm.m_movement_id and mv.confirmationtype='CDW'
WHERE mm.docstatus NOT in('VO','RE') AND mm.m_movement_id = $P{M_Movement_ID}]]>
	</queryString>

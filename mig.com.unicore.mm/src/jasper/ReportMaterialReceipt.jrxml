	<queryString>
		<![CDATA[select ao.Name as Departement,
date (mio.MovementDate) as MovementDate,
mw.Name as WareHouse,
cb.Name as BusinessPartner,
co.documentno as NomorPO,
mio.documentno AS NomorMR,
ml.value as Locator,
mp.name as Product,
miol.QtyEntered as QtyPO,
cu.name as UOM,
miol.MovementQty as QtyGRN,
(miol.QtyEntered - miol.MovementQty) as Difference

from M_InOutLine miol

INNER JOIN M_InOut mio ON mio.M_InOut_ID = miol.M_InOut_ID
LEFT JOIN C_OrderLine col ON col.C_OrderLine_ID = miol.C_OrderLine_ID
LEFT JOIN C_Order co ON co.C_Order_ID = col.C_Order_ID
INNER JOIN AD_Org ao ON ao.ad_org_id = miol.ad_org_id
INNER JOIN M_WareHouse mw ON mw.M_WareHouse_ID = mio.M_WareHOuse_ID
INNER JOIN C_BPartner cb ON cb.C_BPartner_ID = mio.C_BPartner_ID
INNER JOIN M_Locator ml ON ml.M_Locator_ID = miol.M_Locator_ID
INNER JOIN M_Product mp ON mp.M_Product_ID = miol.M_Product_ID
INNER JOIN c_uom cu ON cu.C_UOM_ID = miol.C_Uom_ID

WHERE mio.docstatus = 'CO' AND ao.ad_org_id = $P{AD_Org_ID} AND
(CASE WHEN $P{C_BPartner_ID} IS NOT NULL THEN mio.C_BPartner_ID=$P{C_BPartner_ID} ELSE 1=1 END) AND
mio.MovementDate between $P{StartDate} AND $P{EndDate}

ORDER BY mio.MovementDate, mw.name, cb.name, co.documentno, ml.value]]>
	</queryString>

	<queryString>
		<![CDATA[select ao.name as Departement, mi.movementdate as MovementDate, mw.name as WareHouse, ml.value as Locator, mp.name as Product,mil.QtyInternalUse as QtyInternalUse, cu.name as UOM

from m_inventory mi

inner join ad_org ao ON ao.ad_org_id = mi.ad_org_id

inner join m_warehouse mw ON mw.m_warehouse_id = mi.m_warehouse_id

inner join m_locator ml ON ml.m_locator_id = mw.m_warehouse_id

inner join m_inventoryline mil ON mil.m_inventory_id = mi.m_inventory_id

inner join m_product mp ON mp.m_product_id = mil.m_product_id

inner join c_uom cu ON cu.c_uom_id = mp.c_uom_id

where mil.inventorytype = 'C' AND ao.ad_org_id = $P{AD_Org_ID} AND mi.movementdate between $P{StartDate} AND $P{EndDate}

order by mi.movementdate, mw.name, ml.value, mp.name, mil.QtyInternalUse]]>
	</queryString>

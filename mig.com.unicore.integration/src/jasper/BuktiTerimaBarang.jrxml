	<queryString>
		<![CDATA[SELECT
mt.movementdate AS Tgl,
mt.movementqty AS Qty,
wh.value AS VGudang,
wh.name AS Gudang,
mp.name AS NamaBarang,
bcpc.hscode AS Kode,
(SELECT arl.name
FROM AD_Ref_List arl
where mt.movementtype = arl.value)AS Keterangan,
mp.value AS Value,
uom.x12de355 AS UOM,
bp.name AS BPartner,
bp.value AS VBPartner,
bp.C_Bpartner_ID AS BPartner_ID

FROM M_Transaction mt
INNER JOIN M_Product mp ON mt.M_Product_ID = mp.M_Product_ID
INNER JOIN UNS_BCProductCategory bcpc ON mp.UNS_BCProductCategory_ID = bcpc.UNS_BCProductCategory_ID
INNER JOIN C_UOM uom On mp.C_UOM_ID = uom.C_UOM_ID
INNER JOIN C_Bpartner_Product bpp ON mp.M_Product_ID = bpp.M_Product_ID
INNER JOIN C_Bpartner bp ON bpp.C_Bpartner_ID = bp.C_BPartner_ID
INNER JOIN M_Locator lct ON mt.M_Locator_ID = lct.M_Locator_ID
INNER JOIN M_Warehouse wh ON lct.M_Warehouse_ID = wh.M_Warehouse_ID]]>
	</queryString>

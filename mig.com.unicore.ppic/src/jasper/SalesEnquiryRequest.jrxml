	<queryString>
		<![CDATA[SELECT
us.enquirydate AS enquirydate,
cbp.name AS buyername,
cc.name AS buyercountry,
slb.teu AS teus,
us.dateapproval AS  approvaldate,
slp.qtyavailable AS qtyavailable,
us.shippingperiod_id AS shippingperiod,
slb.fob AS FOB,
slp.volume AS volume,
us.c_period_id AS period,
slp.description AS remark,
cp.name AS TanggalPeriod,
slb.ref_uns_serbuyerproduct_id AS RefNo,
slp.volume*slb.fob AS salestotal

FROM uns_ser us
INNER JOIN uns_serlinebuyer slb ON slb.uns_ser_id = us.uns_ser_id
INNER JOIN uns_serlineproduct slp ON slp.uns_serlinebuyer_id = slb.uns_serlinebuyer_id
INNER JOIN c_bpartner cbp ON cbp.c_bpartner_id = slb.c_bpartner_id
INNER JOIN c_country cc ON cc.c_country_id = slb.c_country_id
INNER JOIN C_Period cp ON us.C_Period_ID = cp.C_Period_ID
WHERE us.C_Period_ID = $P{C_Period_ID}]]>
	</queryString>

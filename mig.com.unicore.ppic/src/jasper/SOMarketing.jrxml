	<queryString>
		<![CDATA[SELECT co.documentno, co.dateordered, co.poreference, co.salesrep_name, co.datepromised,
getuniquecolumn('c_bpartner','name',co.c_bpartner_id) as marketing,
getuniquecolumn('c_bpartner_location','name',co.c_bpartner_location_id) as mktloc,
getuniquecolumn('c_bpartner','value',co.c_bpartnersr_id) as vcust,
getuniquecolumn('c_bpartner','name',co.c_bpartnersr_id) as cust,
getuniquecolumn('uns_ser','documentno',co.uns_ser_id) as ser,
getuniquecolumn('c_bpartner_location','name',co.c_bpartnerrelation_location_id) as custloc,
mp.value, mp.name, mp.pack, ubr.name as brand, odl.qtyentered, odl.priceentered,
cc.cursymbol as currencysymbol, c_uom.name as uom, os.legalrequirement, os.reason,co.shipmark, odl.UOMInCtn
FROM C_Order co
INNER JOIN c_orderline odl ON odl.c_order_id = co.c_order_id
INNER JOIN m_product mp ON odl.m_product_id = mp.m_product_id
LEFT JOIN uns_brand ubr ON ubr.uns_brand_id = mp.uns_brand_id
INNER JOIN c_uom ON odl.c_uom_id = c_uom.c_uom_id
INNER JOIN c_currency cc ON cc.c_currency_id = odl.c_currency_id
LEFT JOIN uns_ordershipment os ON os.c_order_id = co.c_order_id
WHERE co.C_Order_ID= $P{C_Order_ID}]]>
	</queryString>

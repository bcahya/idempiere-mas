	<queryString>
		<![CDATA[SELECT
mp.name AS Product,
iol.qtyEntered AS Qty,
uom.name AS UOM,
io.M_InOut_ID as ioID,
io.dateordered as dateordered

FROM M_InOut io
INNER JOIN M_InOutLine iol ON iol.M_InOut_ID = io.M_InOut_ID
INNER JOIN M_Product mp ON mp.M_Product_ID = iol.M_Product_ID
INNER JOIN C_UOM uom ON uom.C_UOM_ID = iol.C_UOM_ID

WHERE io.dateordered BETWEEN $P{DateFrom} AND $P{DateTo}]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT logo.value, logo.name AS logo, ll.address1, ll.address2, ll.city as lcity, ll.postal, lc.name as lcountry
FROM C_Bpartner logo
LEFT JOIN C_Bpartner_Location lbpl ON lbpl.C_Bpartner_ID=logo.C_Bpartner_ID
INNER JOIN C_Location ll ON lbpl.C_Location_ID=ll.C_Location_ID
INNER JOIN C_Country lc ON lc.C_Country_ID=ll.C_Country_ID
WHERE logo.C_Bpartner_ID=$P{C_BPartner_ID}]]>
	</queryString>

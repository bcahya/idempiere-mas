	<queryString>
		<![CDATA[select up.documentno as DocumentNo, date (up.productiondate) as ProductionDate, ur.name as resource,
mp.name as Product, upd.plannedqty AS PlannedQty, upd.movementqty as MovementQTY, (upd.plannedqty - upd.movementqty) AS Difference, ao.name,

(upd.movementqty / upd.plannedqty) AS Efesiensi,

c_uom.name as UOM, ml.value as Locator

from uns_production up

inner join ad_org ao ON ao.ad_org_id = up.ad_org_id

inner join uns_resource ur ON ur.uns_resource_id = up.uns_resource_id

inner join uns_production_detail upd ON upd.uns_production_id = up.uns_production_id

inner join m_locator ml ON ml.m_locator_id = upd.m_locator_id

inner join m_product mp ON mp.m_product_id = upd.m_product_id

inner join c_uom ON c_uom.c_uom_id = mp.c_uom_id

where ao.ad_org_id = $P{AD_Org_ID} AND upd.isendproduct = 'Y' AND up.productiondate between $P{StartDate} AND $P{EndDate} AND up.docstatus = 'CO'

order by ur.name, up.productiondate, up.documentno, mp.name]]>
	</queryString>

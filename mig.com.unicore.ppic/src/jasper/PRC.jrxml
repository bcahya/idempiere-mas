	<queryString>
		<![CDATA[SELECT c.name as perusahaan, o.name as dept, prc.productiondate, p.value as productkey,prc.codeno,
prc.release, prc.onhold, prc.nc, prc.qty, prc.releaseqty, prc.onholdqty, prc.ncqty, l.remark
FROM UNS_PRC_V prc INNER JOIN UNS_QAStatus_PRCLine l ON l.UNS_QAStatus_PRCLine_ID=prc.UNS_PRC_V
INNER JOIN AD_CLient c ON c.AD_CLient_ID=prc.AD_CLient_ID
INNER JOIN UNS_PRoduction_Detail pd ON pd.UNS_PRoduction_Detail_ID=prc.UNS_PRoduction_Detail_ID
INNER JOIN AD_Org o ON o.AD_Org_ID=pd.AD_Org_id
INNER JOIN M_Product p ON p.M_Product_ID=prc.M_Product_ID
WHERE prc.productiondate=$P{ProductionDate} AND pd.AD_Org_ID=$P{AD_Org_ID}]]>
	</queryString>

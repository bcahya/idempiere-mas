	<queryString>
		<![CDATA[select
ag.name AS Organization,
ci.documentno AS DocumentNo,
ci.dateinvoiced AS Tanggal,
cb.name AS BusinessPartner,
ci.grandtotal AS Total,
ci.grandtotal - invoiceopen(c_invoice_id,0) AS Terpotong,
invoiceopen(c_invoice_id,0) AS SisaPembayaran,
now() AS DatePrint,
CASE WHEN ci.IsSOTrx = 'Y' THEN 'Ya' ELSE 'Tidak' END AS trxType

from C_Invoice ci

INNER JOIN ad_org ag ON ag.ad_org_id=ci.ad_org_id
INNER JOIN C_BPartner cb ON cb.c_bpartner_id=ci.c_bpartner_id

WHERE
(CASE WHEN $P{IsSOTrx} = 'Y' THEN ci.c_doctype_id=1000004 WHEN $P{IsSOTrx} = 'N' THEN ci.c_doctype_id=1000006 ELSE 1=2 END) AND invoiceopen(c_invoice_id,0) > 0 AND
(CASE WHEN $P{AD_Org_ID} IS NOT NULL THEN ci.ad_org_id=$P{AD_Org_ID} ELSE 1=1 END) AND
(CASE WHEN $P{DateFrom}::timestamp IS NOT NULL AND $P{DateTo}::timestamp IS NOT NULL THEN ci.dateinvoiced >= $P{DateFrom}::timestamp AND ci.dateinvoiced <= $P{DateTo}::timestamp WHEN $P{DateTo}::timestamp IS NOT NULL AND $P{DateTo}::timestamp IS NULL THEN ci.dateinvoiced >= $P{DateFrom}::timestamp WHEN $P{DateTo}::timestamp IS NULL AND $P{DateTo}::timestamp IS NOT NULL THEN ci.dateinvoiced <= $P{DateTo}::timestamp ELSE 1=1 END)

GROUP BY ag.name,ci.dateinvoiced,cb.name,ci.documentno,ci.grandtotal,ci.c_invoice_id
ORDER BY ag.name,ci.dateinvoiced,cb.name]]>
	</queryString>

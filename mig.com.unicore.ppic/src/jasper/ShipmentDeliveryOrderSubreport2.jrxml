	</parameter>
	<queryString>
		<![CDATA[SELECT uom.uomsymbol, p.description AS ProductType, sos.QtyUom AS QTYProduct
FROM UNS_SOShipment sos
INNER JOIN UNS_ContainerDeparture cd ON sos.UNS_ContainerDeparture_ID=cd.UNS_ContainerDeparture_ID AND  cd.UNS_ContainerDeparture_ID=$P{UNS_ContainerDeparture_ID}
INNER JOIN UNS_ShipmentSchedule ssc on ssc.UNS_ShipmentSchedule_ID = cd.UNS_ShipmentSchedule_ID AND
ssc.UNS_ShipmentSchedule_ID=$P{UNS_ShipmentSchedule_ID}
INNER JOIN M_Product p on sos.M_Product_ID = p.M_Product_ID
INNER JOIN C_Uom uom on p.c_Uom_ID=uom.c_Uom_ID
INNER JOIN C_OrderLine ol on sos.C_OrderLine_ID = ol.C_OrderLine_ID
INNER JOIN C_Order o on ol.C_Order_ID = o.C_Order_ID AND o.C_Order_ID=$P{C_Order_ID}
ORDER BY ol.C_OrderLine_ID]]>
	</queryString>

	<parameter name="DateFrom" class="java.util.Date"/>
	<parameter name="DateTo" class="java.util.Date"/>
	<parameter name="AD_Org_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
org.name as orgname,
p.name as namaproduksi,
p.documentno as docno,
p.movementdate as mdate,
pd.movementqty as mqty,
pd.uns_production_detail_id as pd_id,
pr.name as produk1,
rs.name as resource,
uom.uomsymbol as uom

FROM uns_production p

INNER JOIN uns_production_detail pd on p.uns_production_id=pd.uns_production_id
INNER JOIN uns_resource rs on p.uns_resource_id=rs.uns_resource_id
INNER JOIN ad_org org on p.ad_org_id=org.ad_org_id
INNER JOIN m_product pr on pd.m_product_id=pr.m_product_id
INNER JOIn c_uom uom on pr.c_uom_id=uom.c_uom_id

WHERE pd.isprimary='Y' and p.docstatus in ('CO','CL') and p.ad_org_id=$P{AD_Org_ID} and p.movementdate between $P{DateFrom} and $P{DateTo}

order by p.movementdate, rs.name]]>
	</queryString>

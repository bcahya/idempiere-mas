	<queryString>
		<![CDATA[SELECT
ps.UNS_PackingSlip_ID AS PAckingSlipID,
pss.UNS_PackingSlipsupplier_ID AS PackingSlipSupplierID,
ps.DocumentNo AS DocNo,
shipper.name AS ShipVia,
ps.PaymentTerm AS FOB,
ps.terms AS Terms,
org.Name AS SalesPerson,
ps.ShipDate AS ShipDate,
ps.DateOrdered AS OrderDate,
ps.SOPOType AS SO,
Cl.name AS OrderedBy,
psl.qtyordered AS orderQty,
uom.name AS Unit,
psl.qtyInvoiced AS incomming,
mp.value AS PMSMCode,
mp.name AS Product,
psl.Description AS Description,
psl.Brutoquantity AS GrossWeight,
psl.Weight AS NettWeight,
bp.Name AS vendor

FROM UNS_PackingSlipLine psl
LEFT JOIN UNS_PackingSlipSupplier pss ON
 pss.UNS_PackingSlipSupplier_ID = psl.UNS_PackingSlipSupplier_ID
LEFT JOIN UNS_PackingSlip ps ON ps.UNS_PackingSlip_ID = pss.UNS_PackingSlip_ID
INNER JOIN AD_Client Cl ON cl.AD_Client_ID = ps.AD_Client_ID
INNER JOIN AD_Org org ON org.AD_Org_ID = ps.AD_Org_ID
INNER JOIN M_Shipper shipper ON shipper.M_Shipper_ID = ps.M_Shipper_ID
INNER JOIN C_UOM uom ON uom.C_UOM_ID = psl.C_UOM_ID
INNER JOIN M_Product mp ON mp.M_Product_ID = psl.M_Product_ID
INNER JOIN C_Bpartner bp ON bp.C_Bpartner_ID = pss.C_Bpartner_ID]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT c.name as perusahaan, c.description as company, bp.name as originalbuyer,
l.address1, l.city, co.name AS country, e.name as employee, j.name as posision,
ss.etd as date, cm.containerno, p.name, p.pack, sosv.coa, sosv.productiondate,
sosv.expiration, sosv.fc, sosv.ph, sosv.dm, sosv.m_attributesetinstance_id,
sosv.UNS_SOShipment_V_ID, sos.qtyuom, uom.uomsymbol as uom
FROM UNS_Employee e INNER JOIN C_Job j ON j.C_Job_ID=e.C_Job_ID,
UNS_SOShipment_V sosv INNER JOIN AD_Client c ON c.AD_Client_ID=sosv.AD_Client_ID
INNER JOIN UNS_ShipmentSchedule ss ON ss.UNS_ShipmentSchedule_ID=sosv.UNS_ShipmentSchedule_ID
INNER JOIN UNS_SOShipment sos ON sos.UNS_SOShipment_id = sosv.UNS_SOShipment_ID
INNER JOIN C_BPartner bp ON bp.C_BPartner_ID=sosv.C_BPartnerSR_ID
INNER JOIN C_Location l ON l.C_Location_ID=sosv.C_Location_ID
INNER JOIN C_Country co ON co.C_Country_ID=l.C_Country_ID
INNER JOIN UNS_ContainerManagement cm ON cm.UNS_ContainerManagement_ID=sosv.UNS_ContainerManagement_ID
INNER JOIN M_Product p ON p.M_Product_ID=sosv.M_Product_ID
INNER JOIN C_Uom uom ON sos.C_Uom_ID=uom.C_Uom_ID
WHERE sosv.UNS_ShipmentSchedule_ID=$P{UNS_ShipmentSchedule_ID} AND e.UNS_Employee_ID=$P{UNS_Employee_ID} AND ss.DocStatus='CO']]>
	</queryString>

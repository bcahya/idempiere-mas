	<parameter name="C_Invoice_ID" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select ci.documentno,
     cbp.name AS companyname,
     cl.address1 as address1,
     cl.address2 as address2,
     mpc.name as category,
     u_kap.name,
     u_cdp.blno,
     cbpl.name,
     ci.dateinvoiced,
     ord.documentno,
     mp.value as productid,
     cil.qtyinvoiced,
     cil.pricelist,
     cil.pricelist * cil.qtyinvoiced as amount,
     coalesce(uom.uomsymbol,'') as uomsymbol,
     ivc_bc.c_invoiceline_id,
     coalesce(cur.iso_code,'') as iso_code,
     client.description,
     getsayinwords(ci.grandtotal),
     bpl.name as location
from c_invoice ci
     join C_InvoiceLine cil on cil.c_invoice_id=ci.c_invoice_id
     join AD_Client client on client.ad_client_id = ci.ad_client_id
     --get partner
     join c_bpartner cbp on cbp.c_bpartner_id = ci.c_bpartner_id
     join c_bpartner_location cbpl on cbpl.c_bpartner_id = cbp.c_bpartner_id
     join c_location cl on cl.c_location_id = cbpl.c_location_id
     --get shipment
     join UNS_ShipmentSchedule u_shp on u_shp.uns_shipmentschedule_id =ci.uns_shipmentschedule_id
     join UNS_Kapal u_kap on u_kap.uns_kapal_id = u_shp.uns_kapal_id
     --get order line
     join UNS_ContainerDeparture u_cdp on u_cdp.uns_shipmentschedule_id = u_shp.uns_shipmentschedule_id
     join UNS_SOShipment u_s_shp on u_s_shp.uns_containerdeparture_id = u_cdp.uns_containerdeparture_id
     join C_OrderLine col on col.c_orderline_id = u_s_shp.c_orderline_id
     --get product shipment
     join M_Product mp on mp.m_product_id = col.m_product_id and mp.m_product_id = cil.m_product_id
     join m_product_category mpc on mpc.m_product_category_id=mp.m_product_category_id
     --get order number ---> done
     join C_Order ord on ord.c_order_id=col.c_order_id
     --get currentcy other
     left join UNS_InvoiceBC ivc_bc on cil.c_invoiceline_id = ivc_bc.c_invoiceline_id
     join c_uom uom on uom.c_uom_id = cil.c_uom_id
     join C_Currency cur on cur.c_currency_id  = ci.c_currency_id,
     c_bpartner bp join c_bpartner_Location bpl on bp.c_bpartner_id=bpl.c_bpartner_id
 WHERE ci.c_invoice_id = $P{C_Invoice_ID} and bp.value like 'PSG'
 order by mp.value]]>
	</queryString>

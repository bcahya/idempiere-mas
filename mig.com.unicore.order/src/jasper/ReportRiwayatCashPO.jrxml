	<queryString>
		<![CDATA[SELECT
o.DocumentNo AS NoPO,
o.DateOrdered AS TglPO,
o.DocStatus AS StatusPO,
bp.Name AS MitraBisnis,
coalesce (py.docstatus,'-') as payment,
coalesce (io.documentno,'-') as noterima,
io.movementdate as tglterima,
coalesce(io.docstatus,'-') as statusterima,
coalesce (wh.Name,'-') as gudang,
coalesce(py.documentno,'-') as nodocbayar

FROM C_Order o

INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = o.C_BPartner_ID
LEFT OUTER JOIN C_PaymentAllocate pa ON pa.C_order_ID=o.C_Order_ID
LEFT OUTER JOIN C_Payment py ON py.C_Payment_ID=pa.C_Payment_ID
LEFT OUTER JOIN M_InOut io ON io.C_Order_ID=o.C_Order_ID
LEFT OUTER JOIN M_Warehouse wh ON wh.M_Warehouse_ID=io.M_Warehouse_ID

WHERE o.C_DocType_ID=1000546 AND

o.DateOrdered BETWEEN $P{DateFrom} AND $P{DateTo} AND (CASE WHEN $P{AD_Org_ID} IS NOT NULL THEN o.AD_Org_ID = $P{AD_Org_ID} ELSE 1=1 END)

ORDER BY o.DocStatus]]>
	</queryString>

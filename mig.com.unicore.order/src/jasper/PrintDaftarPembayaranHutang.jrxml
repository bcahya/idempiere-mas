		<![CDATA[select py.documentno as No_Bukti, py.dateacct as Tanggal,bp.name,inv.c_invoice_id as no_faktur, inv.dateinvoiced as tgl_faktur,inv.grandtotal as faktur , invoiceopen(inv.c_invoice_id,null) as sisa, inv.grandtotal - invoiceopen(inv.c_invoice_id,null) as bayar, now() as tgl_print  from c_allocationline acl join c_payment py on (acl.c_payment_id = py.c_payment_id) join c_invoice inv on (acl.c_invoice_id = inv.c_invoice_id) join c_bpartner bp on (inv.c_bpartner_id = bp.c_bpartner_id) join c_doctype dt on (py.c_doctype_id = dt.c_doctype_id ) where inv.c_invoice_id = acl.c_invoice_id and bp.isvendor = 'Y' AND dt.name = 'AP Payment' and py.docstatus = 'CO' and py.C_Payment_ID = $P{C_Payment_ID}]]>

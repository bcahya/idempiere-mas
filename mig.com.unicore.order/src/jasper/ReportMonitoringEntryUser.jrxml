	<property name="ireport.y" value="0"/>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<queryString>
		<![CDATA[select ci.documentno nofaktur,ci.dateinvoiced tglfaktur,ci.grandtotal as tfak,cb.name nm_customer,u.name as sales,
(select us.name from ad_user as us where ci.createdby=us.ad_user_id) as nmuser
from c_invoice as ci
left join c_bpartner as cb on ci.c_bpartner_id=cb.c_bpartner_id
left join ad_user as u on u.ad_user_id=ci.salesrep_id
where ci.dateinvoiced>=$P{StartDate} and ci.dateinvoiced<=$P{EndDate}
group by ci.documentno,ci.dateinvoiced,ci.grandtotal,cb.name,u.name,ci.createdby
order by nmuser,sales,tglfaktur]]>
	</queryString>
	<field name="nofaktur" class="java.lang.String"/>
	<field name="tglfaktur" class="java.sql.Timestamp"/>
	<field name="tfak" class="java.math.BigDecimal"/>
	<field name="nm_customer" class="java.lang.String"/>
	<field name="sales" class="java.lang.String"/>
	<field name="nmuser" class="java.lang.String"/>
	<variable name="Nourut" class="java.lang.Integer" resetType="Group" resetGroup="Salesman" calculation="Count">

	<queryString>
		<![CDATA[select mi.documentno as noshipcus,mi.movementdate as tglshipcus,jo.documentno as nojob,jo.datedoc as tgljob,jr.documentno nojobrec,jr.datedoc as tgljobrec,
 shf.documentno as noshipping,shf.datedoc as tglshipping,shf.driver as supir,ci.dateinvoiced as tglinvoice,ci.documentno as noinvoice,cb.name as nmcust
 from M_InOut mi
 left join UNS_ExpeditionDetail exp on mi.m_inout_id=exp.m_inout_id
 left join UNS_Expedition jo on exp.uns_expedition_id=jo.uns_expedition_id
 left join UNS_Expedition jr on jr.jobso_id=exp.uns_expedition_id
 left join UNS_ShippingFreight sff on sff.uns_expedition_id=jr.uns_expedition_id
 left join uns_shipping shf on sff.uns_shipping_id=shf.uns_shipping_id
 inner join ad_org org on mi.ad_org_id=org.ad_org_id
 inner join c_invoice ci on mi.c_invoice_id=ci.c_invoice_id
 inner join c_bpartner cb on ci.c_bpartner_id=cb.c_bpartner_id
 where mi.isdropship='Y'
 and mi.docstatus in('CO','CL') and mi.movementdate between $P{StartDate} and $P{EndDate}
and
(Case When $P{C_SalesRegion_ID} is not null then org.ad_org_id in(select ad_org_id from ad_org where c_salesregion_id=$P{C_SalesRegion_ID}) else 1=1 end)
order by ci.dateinvoiced]]>
	</queryString>

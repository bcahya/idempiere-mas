	<parameter name="UNS_PackingList_ID" class="java.lang.Integer"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
	ROUND(COALESCE(il.uomqtyinvoicedl1,0)) AS qty1,
	ROUND(COALESCE(il.uomqtyinvoicedl2,0)) AS qty2,
	ROUND(COALESCE(il.uomqtyinvoicedl3,0)) AS qty3,
	ROUND(COALESCE(il.uomqtyinvoicedl4,0)) AS qty4,
     ( SELECT COALESCE(UOMSymbol, name) FROM c_uom WHERE c_uom_id = mp.C_UOM_ID ) as satuan,
     ( SELECT COALESCE(UOMSymbol, name) FROM c_uom WHERE c_uom_id = mp.uomconversionl1_id) as Satuan1,
     ( SELECT COALESCE(UOMSymbol, name) FROM c_uom WHERE c_uom_id = mp.uomconversionl2_id) as satuan2,
     ( SELECT COALESCE(UOMSymbol, name) FROM c_uom WHERE c_uom_id = mp.uomconversionl3_id) as satuan3,
     ( SELECT COALESCE(UOMSymbol, name) FROM c_uom WHERE c_uom_id = mp.uomconversionl4_id) as satuan4,
     ( SELECT name FROM ad_user where ad_user_id= $P{AD_User_ID}) as user,
COALESCE((CASE WHEN il.isProductBonuses ='N' THEN calcdiscclaiminv(il.C_InvoiceLine_ID, 'Y') WHEN il.isProductBonuses='Y' THEN calcdiscbonusesinv(il.C_InvoiceLine_ID, 'Y') ELSE 0 END),0) AS DiscClaim,

COALESCE((CASE WHEN il.isProductBonuses ='N' THEN (il.DiscountAmt - COALESCE(calcdiscclaiminv(il.C_InvoiceLine_ID, 'Y'),0)) WHEN il.isProductBonuses='Y' THEN (il.DiscountAmt - COALESCE(calcdiscbonusesinv(il.C_InvoiceLine_ID, 'Y'),0)) ELSE 0 END),0) AS DiscNoClaim,

     inv.grandtotal as Gtot,
     inv.adddiscountamt as DisFak,
     inv.documentno AS NoFaktur,
     inv.dateinvoiced AS TglFaktur,
     bp.value as kd_cust,
     bpl.bpartnername AS NamaToko,
     il.pricelist AS Pricelist,
     il.pricelist*il.qtyinvoiced AS jumlah,
     mp.value AS KodeProduk,
     left(concat(mp.value,'-',mp.name),32) AS NmProduk,
     COALESCE((SELECT SUM(taxamt) FROM C_InvoiceTax WHERE C_Invoice_ID = inv.C_Invoice_ID),0) AS ppn,
     COALESCE(emp.value, usr.Name) AS KDSales,
     usr.name AS NamaSales,
     concat_ws(' ',coalesce(cl.address1,''),coalesce(cl.address2,''),coalesce(cl.address3,''),coalesce(cl.address4,''), coalesce(bpl.phone,'')) as al,
     org.name AS org,
     org.value as Aorg,
     co.documentno as noorder,
     inv.description As Catatan,
     io.documentno as shipment,
     inv.dateinvoiced +interval '1 days' *(select netdays from c_paymentterm where c_paymentterm_id=inv.c_paymentterm_id) as TglJtempo,
     (select name from c_paymentterm where c_paymentterm_id=inv.c_paymentterm_id)  as Carabayar,
case when inv.paymentrule='B' then 'CASH BEFORE DELIVERY' else '' end as cash,
--     case when(select name from c_paymentterm where c_paymentterm_id=inv.c_paymentterm_id)='TOP 1 Day' then 'COD' else '' end  as cash,
     getterbilangrupiah(inv.grandtotal) as terbilang
FROM
	uns_packinglist pl
     INNER JOIN uns_packinglist_order plo ON plo.uns_packinglist_id = pl.uns_packinglist_id
     INNER JOIN c_invoice inv ON plo.c_invoice_id = inv.c_invoice_id
     LEFT JOIN c_invoiceline il ON il.c_invoice_id = inv.c_invoice_id
     INNER JOIN m_product mp ON mp.m_product_id = il.M_Product_ID
     INNER JOIN c_bpartner bp ON bp.c_bpartner_id = inv.c_bpartner_id
     INNER JOIN ad_user usr ON inv.salesrep_id = usr.ad_user_id
     LEFT JOIN uns_employee emp ON usr.uns_employee_id = emp.uns_employee_id
     INNER JOIN c_bpartner_location bpl on inv.c_bpartner_location_id=bpl.c_bpartner_location_id and bp.c_bpartner_id = bpl.c_bpartner_id
     INNER JOIN ad_org org on inv.ad_org_id = org.ad_org_id
     INNER JOIN c_order co on co.c_order_id=inv.c_order_id
     INNER JOIN c_location cl on cl.c_location_id=bpl.c_location_id
     inner join m_inout io on plo.m_inout_id=io.m_inout_id
WHERE
     pl.uns_packinglist_id = $P{UNS_PackingList_ID}
and inv.docstatus NOT IN ('VO', 'RE')
order by inv.documentno, mp.value]]>
	</queryString>

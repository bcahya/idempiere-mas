	<parameter name="C_BPartner_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
bp.name AS namatoko,
pd.name AS namabrg,
oql.qty AS qtybrg,
ord.documentno AS nord

FROM
uns_orderqueue oq
INNER JOIN uns_orderqueue_line oql ON oql.uns_orderqueue_id=oq.uns_orderqueue_id
INNER JOIN m_product pd on pd.m_product_id=oq.m_product_id
INNER JOIN c_bpartner bp ON bp.c_bpartner_id=oql.c_bpartner_id
INNER JOIN c_orderline orl ON orl.c_orderline_id=oql.c_orderline_id
INNER JOIN c_order ord ON ord.c_order_id=orl.c_order_id

WHERE
oql.c_bpartner_id = $P{C_BPartner_ID}]]>
	</queryString>
	<field name="namatoko" class="java.lang.String"/>
	<field name="namabrg" class="java.lang.String"/>
	<field name="qtybrg" class="java.math.BigDecimal"/>
	<field name="nord" class="java.lang.String"/>

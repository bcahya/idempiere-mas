	</parameter>
	<queryString>
		<![CDATA[SELECT rpt.DocumentNo AS DocNo, COALESCE(bpl.BPartnerName, cc.Name) AS Customer, rpt.DateDoc AS DateDoc, usr.Name AS Sales, rpt.DateDoc + 1 AS DueDate, p.Name AS Product, UOMConversionToStr(p.M_Product_ID, pro.QtySold) AS Qty, pro.PriceActual AS Price, pro.LineNetAmt AS LineAmount, cc.UNS_CvsCustomer_ID, TRIM(GetSimpleAddress(COALESCE(bpl.C_Location_ID, ccloc.C_Location_ID))) AS Address, adm.Name AS Adm,
getterbilangrupiah(pro.LineNetAmt) AS AmountInWords

FROM UNS_Cvs_Rpt rpt

INNER JOIN UNS_Cvs_RptCustomer cmr ON cmr.UNS_Cvs_Rpt_ID = rpt.UNS_Cvs_Rpt_ID
LEFT JOIN UNS_CvsCustomer cc ON cc.UNS_CvsCustomer_ID = cmr.UNS_CvsCustomer_ID
LEFT JOIN UNS_CvsCustomer_Location ccloc ON ccloc.UNS_CvsCustomer_Location_ID = cmr.UNS_CvsCustomer_Location_ID
LEFT JOIN C_BPartner_Location bpl ON bpl.C_BPartner_Location_ID = cmr.C_BPartnerRelation_Location_ID
INNER JOIN UNS_Cvs_RptProduct pro ON pro.UNS_Cvs_RptCustomer_ID = cmr.UNS_Cvs_RptCustomer_ID
INNER JOIN M_Product p ON p.M_Product_ID = pro.M_Product_ID
INNER JOIN AD_User usr ON usr.AD_User_ID = rpt.SalesRep_ID
INNER JOIN AD_User adm ON adm.AD_User_ID = $P{AD_User_ID}

WHERE rpt.UNS_Cvs_Rpt_ID = $P{UNS_Cvs_Rpt_ID}

ORDER BY Customer, Product]]>
	</queryString>

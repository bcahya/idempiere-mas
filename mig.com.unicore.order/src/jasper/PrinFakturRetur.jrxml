	<queryString>
		<![CDATA[SELECT
     ROUND(coalesce(c_invoiceline."uomqtyenteredl1",'0'),0) AS qty1,
     ROUND(coalesce(c_invoiceline."uomqtyenteredl2",'0'),0) AS qty2,
     ROUND(coalesce(c_invoiceline."uomqtyenteredl3",'0'),0) AS qty3,
     ROUND(coalesce(c_invoiceline."uomqtyenteredl4",'0'),0) AS qty4,
     ROUND(coalesce(c_invoiceline."uomqtybonusesl1",'0'),0) AS qtyb1,
     ROUND(coalesce(c_invoiceline."uomqtybonusesl2",'0'),0) AS qtyb2,
     ROUND(coalesce(c_invoiceline."uomqtybonusesl3",'0'),0) AS qtyb3,
     ROUND(coalesce(c_invoiceline."uomqtybonusesl4",'0'),0) AS qtyb4,
     case when(c_invoiceline."isproductbonuses"='Y')then c_invoiceline."priceactual" ELSE 0 end AS Diskonbrg,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl1_id" ) as Satuan1,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl2_id" ) as satuan2,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl3_id" ) as satuan3,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl4_id" ) as satuan4,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl1_id" ) as Satuanb1,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl2_id" ) as satuanb2,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl3_id" ) as satuanb3,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_invoiceline."uomconversionl4_id" ) as satuanb4,
     ( SELECT "ad_user"."name" FROM "ad_user" where ad_user_id= $P{AD_User_ID}) as user,
     c_invoice."grandtotal" as Gtot,
     c_invoice."adddiscountamt" as DisFak,
     c_invoice."documentno" AS NoFaktur,
     c_invoice."dateinvoiced" AS TglFaktur,
     c_bpartner."name" AS NamaToko,
     c_invoiceline."pricelist" AS Pricelist,
     c_invoiceline."linenetamt" AS jumlah,
     COALESCE(c_invoiceline."discountamt",0) AS Dicount,
     m_product."value" AS KodeProduk,
     m_product."name" AS NmProduk,
     SUM(c_invoicetax."taxamt") AS ppn,
     emp."value" AS KDSales,
     ad_user."name" AS NamaSales,
     c_bpartner_location."name" AS Alamat,
     ad_org."name" AS org,
     ad_org."value" as Aorg,
     c_order."documentno" as noorder,
     c_invoice."description" As Catatan,
     c_invoice.dateinvoiced +interval '1 days' *(select "c_paymentterm"."netdays" from "c_paymentterm" where c_paymentterm_id=c_invoice."c_paymentterm_id") as TglJtempo,
     (select name from "c_paymentterm" where c_paymentterm_id=c_invoice."c_paymentterm_id")  as Carabayar,
     case when(select name from "c_paymentterm" where c_paymentterm_id=c_invoice."c_paymentterm_id")='TOP 1 Day' then 'CASH' else '' end  as cash,
     getterbilangrupiah(c_invoice."grandtotal") as terbilang
FROM
     "adempiere"."c_invoice"
     inner join "adempiere"."c_invoiceline" ON c_invoice."c_invoice_id" = c_invoiceline."c_invoice_id"
     INNER JOIN "adempiere"."m_product" m_product ON c_invoiceline."m_product_id" = m_product."m_product_id"
     INNER JOIN "adempiere"."c_bpartner" c_bpartner ON c_bpartner."c_bpartner_id" = c_invoice."c_bpartner_id"
     LEFT JOIN "adempiere"."c_invoicetax" c_invoicetax ON c_invoice."c_invoice_id" = c_invoicetax."c_invoice_id"
     LEFT JOIN "adempiere"."ad_user" ad_user ON c_invoice."salesrep_id" = ad_user."ad_user_id"
     LEFT JOIN "adempiere"."uns_employee" emp ON ad_user."uns_employee_id" = emp."uns_employee_id"
     LEFT JOIN "adempiere"."c_bpartner_location" c_bpartner_location  on c_invoice."c_bpartner_location_id"=c_bpartner_location."c_bpartner_location_id"
     LEFT JOIN "adempiere"."ad_org" ad_org on c_invoice."ad_org_id" = ad_org."ad_org_id"
	LEFT JOIN "adempiere"."c_order" c_order on c_order."c_order_id"=c_invoice."c_order_id"
WHERE
     c_invoice."c_invoice_id" =  $P{C_Invoice_ID}
and c_invoice."docstatus" in('CO','CL') AND (c_invoice.c_doctype_id IN ( SELECT c_doctype.c_doctype_id FROM c_doctype WHERE c_doctype.docbasetype = 'ARC'))
group by c_invoiceline.c_invoiceline_id, c_invoice.c_invoice_id, c_bpartner.c_bpartner_id, m_product.m_product_id, emp.uns_employee_id, ad_user.ad_user_id, c_bpartner_location.c_bpartner_location_id, ad_org.ad_org_id, c_order.c_order_id
order by c_invoice."documentno"]]>
	</queryString>

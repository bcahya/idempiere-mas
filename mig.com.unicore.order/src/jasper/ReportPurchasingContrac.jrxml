	<parameter name="Documentno" class="java.lang.String"/>
	<queryString>
		<![CDATA[select con.documentno as nocontrack,con.daterequired as tglkontrak,con.description as keterangan,cb.name as namavendor,cl.qty as qtykontrak,co.documentno as nopo,co.dateordered as tglorder,uomconversiontostr(mp.m_product_id,col.qtyordered) as qtyorder,mp.name as nmproduct,cl.qty-cl.qtyordered as sisa
 from M_Requisition con
 inner join M_RequisitionLine cl on con.m_requisition_id=cl.m_requisition_id
 inner join c_orderline col on cl.m_requisitionline_id=col.m_requisitionline_id
 inner join m_product mp on col.m_product_id=mp.m_product_id
 inner join c_order co on col.c_order_id=co.c_order_id
 left join c_bpartner cb on cl.c_bpartner_id=cb.c_bpartner_id
 where con.docstatus in('CO','CL') and con.documentno=$P{Documentno}]]>

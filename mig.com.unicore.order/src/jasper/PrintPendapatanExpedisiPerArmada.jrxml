	<queryString>
		<![CDATA[SELECT now() as tglskrg,u.name as uctk,org.name as org,xp.documentno as spa,xp.datedoc as tgl,am.name as aramada,at.name as atype,ci.grandtotal as tagihan,bl.bpartnername as nmcus,
coalesce(lc.address1,'-') as asal,coalesce(lc.city,'-') as aslkota,coalesce(la.address1,'-') as taalamat,coalesce(la.city,'-') as takota,cg.name as charge,cd.amountconfirmed as rtcharge,cd.amount as tcharge,sh.grandtotal as tcost,sh.documentno as docno
FROM uns_expedition xp
left join c_invoice ci on ci.c_invoice_id=xp.c_invoice_id
left join c_bpartner_location bl on bl.c_bpartner_location_id=ci.c_bpartner_location_id
left join UNS_ShippingFreight sfr on sfr.uns_expedition_id=xp.uns_expedition_id
left join uns_shipping sh on sh.uns_shipping_id=sfr.uns_shipping_id
left join uns_armada am on am.uns_armada_id=sh.uns_armada_id
left join uns_armadatype at on at.uns_armadatype_id=am.uns_armadatype_id
left join UNS_Charge_Detail cd on cd.uns_shipping_id=sh.uns_shipping_id and cd.iscancelled='N'
left join c_charge cg on cg.c_charge_id=cd.c_charge_id
left join c_location lc on lc.c_location_id=xp.origin_id
left join c_location la on la.c_location_id=xp.destination_id
left join ad_org org on org.ad_org_id=xp.ad_org_id
left join ad_user u on u.ad_user_id=$P{AD_User_ID}
where xp.docstatus in('CO','CL')
and xp.datedoc between $P{StartDate} and $P{EndDate}
order by org.name,xp.datedoc,xp.documentno,am.name]]>
	</queryString>

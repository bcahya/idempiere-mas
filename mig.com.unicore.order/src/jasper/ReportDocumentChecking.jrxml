	<queryString>
		<![CDATA[select docs.name as nmdocu,doc.documentno as noopanem,doc.datedoc as tgldocument,us.name as nam_sales,docl.documentno as nodoc,docl.amount as nilai,coalesce(refl.name,'-') as alasan,
 coalesce(refls.name,'-') as status,coalesce(ry.name,'') as nmrayon,cbl.bpartnername as nmcustomer
 from UNS_DocChecking doc
 left join uns_doccheckingline docl on doc.uns_docchecking_id=docl.uns_docchecking_id
 left join ad_user us on us.ad_user_id=doc.salesrep_id
 left join uns_documentschema docs on docs.uns_documentschema_id=doc.uns_documentschema_id
 left join c_bpartner_location cbl on cbl.c_bpartner_location_id=docl.c_bpartner_location_id
 left join uns_rayon ry on ry.uns_rayon_id=doc.uns_rayon_id
 left join AD_Ref_List refl on refl.value=docl.reason
 left join AD_Ref_LIst refls on refls.value=docl.status
  where doc.UNS_DocChecking_ID=$P{UNS_DocChecking_ID}]]>

	<queryString>
		<![CDATA[select
	org.name as org,
	org.description as orgdescription,
	orgloc.address1 as address,
	orginfo.phone as phone,
	orginfo.fax as fax,
	vendor.name as vendor,
	vendorregion.name as vendorregion,
	armada.name as carnumber,
	armadatype.name as armadatype,
	cd.vouchercode as vouchercode,
	ship.datedoc as datedoc,
	ship.driver as driver
from
	uns_shipping ship
inner join
	ad_org org
	on
	org.ad_org_id = ship.ad_org_id
inner join
	ad_orginfo orginfo
	on
	 orginfo.ad_org_id = org.ad_org_id
left join
	c_location orgloc on orgloc.c_location_id = orginfo.c_location_id
inner join
	uns_charge_detail cd
	on
	cd.uns_shipping_id = ship.uns_shipping_id
left join
	c_bpartner vendor
	on
	vendor.c_bpartner_id = cd.c_bpartner_id
left join
	c_bpartner_location bploc
	on
	bploc.c_bpartner_location_id = cd.c_bpartner_location_id
left join
	uns_armada armada
	on
	armada.uns_armada_id = ship.uns_armada_id
left join
	uns_armadatype armadatype
	on
	armadatype.uns_armadatype_id = armada.uns_armadatype_id
left join
	c_region vendorregion
	on
	vendorregion.c_region_id = bploc.c_location_id
where
	ship.uns_shipping_id =  $P{UNS_Shipping_ID}
and
	cd.chargetype = 'VC'
and
	cd.isfuel = 'N']]>
	</queryString>

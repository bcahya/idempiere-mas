	<queryString>
		<![CDATA[SELECT pc.DocumentNo AS NoPLConfirm, pl.DocumentNo AS NoPL, bp.Name AS Customer, mp.Name AS Product, pll.MovementQty AS Qty, COALESCE (r.Name, r.Name, '') AS Rayon  FROM UNS_PL_Confirm pc

INNER JOIN UNS_PackingList pl ON pl.UNS_PackingList_ID = pc.UNS_PackingList_ID

INNER JOIN UNS_PackingList_Order po ON po.UNS_PackingList_ID = pl.UNS_PackingList_ID

INNER JOIN C_Order o ON o.C_Order_ID = po.C_Order_ID

INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = o.C_BPartner_ID

INNER JOIN C_BPartner_Location bpl ON bpl.C_BPartner_ID = bp.C_BPartner_ID

LEFT JOIN UNS_Rayon r ON r.UNS_Rayon_ID = bpl.UNS_Rayon_ID

INNER JOIN UNS_PackingList_Line pll ON pll.UNS_PackingList_Order_ID = po.UNS_PackingList_Order_ID

INNER JOIN M_Product mp ON mp.M_Product_ID = pll.M_Product_ID

WHERE pc.DateDoc BETWEEN $P{StartDate} AND $P{EndDate} AND pc.Docstatus IN ('DR', 'IN') AND pc.AD_Org_ID = $P{AD_Org_ID}]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT org.name as orgname,mi.documentno,mi.movementdate,mp.name as nmproduc,cg.name as nm_charge,uomconversiontostr(mp.m_product_id,ml.qtyinternaluse) as qty,coalesce(ml.description) as ket
 FROM M_Inventory mi
 inner join M_InventoryLine ml on mi.m_inventory_id=ml.m_inventory_id
 inner join c_charge cg on cg.c_charge_id=ml.c_charge_id
 inner join m_product mp on mp.m_product_id=ml.m_product_id
 inner join ad_org org on org.ad_org_id=mi.ad_org_id
 WHERE  mi.M_Inventory_ID=$P{M_Inventory_ID}]]>

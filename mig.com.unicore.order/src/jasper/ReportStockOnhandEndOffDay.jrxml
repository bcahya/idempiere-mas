<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ReportStockOnhandEndOffDay" language="groovy" pageWidth="612" pageHeight="792" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="b37fa7e3-45e1-47b4-8afe-42adf8dc7bbb">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<parameter name="M_Warehouse_ID" class="java.lang.Integer"/>
	<parameter name="M_Product_ID" class="java.lang.Integer"/>
	<parameter name="M_Locator_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select now() as tglskrg,wh.value as nama_gudang,u.name as nama,m.name as nm_barang,data.sm_product_id,loc.value as nmlok,
round(coalesce(m.uomqtyl1,0),0) as isi1,
round(coalesce(m.uomqtyl2,0),0) as isi2,
round(coalesce(m.uomqtyl3,0),0) as isi3,
round(coalesce(m.uomqtyl4,0),0) as isi4,
coalesce((select cu.name from c_uom cu where cu.c_uom_id=m.uomconversionl1_id),'') as satuan1,
coalesce((select cu.name from c_uom cu where cu.c_uom_id=m.uomconversionl2_id),'') as satuan2,
coalesce((select cu.name from c_uom cu where cu.c_uom_id=m.uomconversionl3_id),'') as satuan3,
coalesce((select coalesce(cu.name,'0') from c_uom cu where cu.c_uom_id=m.uomconversionl4_id),'') as satuan4,
data.qty,data.onhand as onhand,
uomconversiontostr(data.sm_product_id,data.onhand-data.qty) sqty,sum(t.movementqty) as qt,uomconversiontostr(data.sm_product_id,(sum(t.movementqty))) as sqt,
(data.onhand-data.qty) as saldo,
uomconversiontostr(data.sm_product_id,data.onhand-data.qty) as ssaldo,
(data.onhand-data.qty)+sum(t.movementqty) as sakhir,
uomconversiontostr(data.sm_product_id,(data.onhand-data.qty)+sum(t.movementqty)) as ssakhir
from (
select stock.sm_product_id,stock.sm_locator_id,stock.qty,sum(mo.qtyonhand) as onhand
from (select mt.m_product_id as sm_product_id,ml.m_locator_id as sm_locator_id,sum(mt.movementqty) as qty
from m_transaction mt
left join m_locator ml on ml.m_locator_id=mt.m_locator_id
left join m_warehouse wh on wh.m_warehouse_id=ml.m_warehouse_id
where mt.movementdate>=$P{StartDate}
AND case when $P{M_Warehouse_ID}!=0 then wh.m_warehouse_id=$P{M_Warehouse_ID} else 1=1 end
AND case when $P{M_Product_ID}!=0 then mt.m_product_id=$P{M_Product_ID} else 1=1 end
and case when $P{M_Locator_ID}!=0 then ml.m_locator_id=$P{M_Locator_ID} else 1=1 end
group by mt.m_product_id,ml.m_locator_id) as stock
left join m_storageonhand mo on stock.sm_product_id=mo.m_product_id and stock.sm_locator_id=mo.m_locator_id
group by stock.sm_locator_id,stock.sm_product_id,stock.qty) as data
left join m_transaction t on t.m_product_id=data.sm_product_id and t.m_locator_id=data.sm_locator_id
left join m_product m on m.m_product_id=t.m_product_id
left join m_locator loc on loc.m_locator_id=t.m_locator_id
left join m_warehouse wh on wh.m_warehouse_id=loc.m_warehouse_id
left join ad_user u on u.ad_user_id=$P{AD_User_ID}
where t.movementdate between $P{StartDate} and $P{EndDate} and t.movementqty<>0
group by u.name,wh.value,m.name,loc.value,data.sm_product_id,data.qty,data.onhand,m.uomqtyl1,m.uomqtyl2,m.uomqtyl3,m.uomqtyl4,m.uomconversionl1_id,m.uomconversionl2_id,m.uomconversionl3_id,m.uomconversionl4_id
order by wh.value,loc.value]]>
	</queryString>

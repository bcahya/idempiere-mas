	<queryString>
		<![CDATA[SELECT sh.DocumentNo AS Number, exp.DocumentNo as DO, sh.DateDoc AS Date, ar.Name AS Armada, ei.ProductName AS Item, (CASE WHEN ei.M_Product_ID > 0 THEN uomconversiontostr(ei.M_Product_ID, ei.Qty) ELSE CEILING(ei.Qty) || ' ' || COALESCE(UOM.UOMSymbol, UOM.Name) END) AS QtyUOM, usr.Name AS Adm, sh.Driver AS Driver, COALESCE(ei.Description,'') AS desc, es.UNS_ExpeditionSign_ID AS signid, es.BPartnerName AS Partner, es.Address AS Address
FROM UNS_Shipping sh
INNER JOIN UNS_Armada ar ON ar.UNS_Armada_ID = sh.UNS_Armada_ID
INNER JOIN AD_User usr ON usr.AD_User_ID = $P{AD_User_ID}
INNER JOIN UNS_ShippingFreight sf ON sf.UNS_Shipping_ID = sh.UNS_Shipping_ID
INNER JOIN UNS_Expedition exp ON exp.UNS_Expedition_ID = sf.UNS_Expedition_ID
INNER JOIN UNS_ExpeditionDetail ed ON ed.UNS_Expedition_ID = exp.UNS_Expedition_ID
INNER JOIN UNS_ExpeditionSign es ON es.UNS_ExpeditionDetailSO_ID = ed.UNS_ExpeditionDetail_ID
INNER JOIN UNS_ExpeditionItems ei ON ei.UNS_ExpeditionSign_ID = es.UNS_ExpeditionSign_ID
INNER JOIN C_UOM uom ON uom.C_UOM_ID = ei.C_UOM_ID

WHERE sh.UNS_Shipping_ID = $P{UNS_Shipping_ID}

ORDER BY TRIM(es.BPartnerName) ASC]]>
	</queryString>

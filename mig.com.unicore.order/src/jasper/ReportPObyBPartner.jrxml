	<queryString>
		<![CDATA[select co.documentno as NomorPO, ao.name as Departement, date (col.dateordered) as DateOrdered, cb.name as BusinessPartner, mw.name as WareHouse, mp.name as Product, col.qtyentered as QTY, cu.name as UOM, col.priceactual as unitprice, (col.priceactual * col.qtyentered) as Amount, col.discountamt as Discount, col.linenetamt as GrandTotal, date (col.dateordered) as DateOrdered, co.grandtotal as Summary

from C_OrderLine col

inner join C_Order co ON co.C_Order_ID = col.C_Order_ID

inner join ad_org ao ON ao.ad_org_id = col.ad_org_id

inner join C_BPartner cb ON cb.C_BPartner_ID = co.C_BPartner_ID

inner join M_Warehouse mw ON mw.M_Warehouse_ID = co.M_Warehouse_ID

inner join M_Product mp ON mp.M_Product_ID = col.M_Product_ID

inner join c_uom cu ON cu.c_uom_id = col.c_uom_id

where isSOTrx = 'N' AND co.docstatus = 'CO' AND ao.ad_org_id = $P{AD_Org_ID} AND col.dateordered between $P{StartDate} AND $P{EndDate}

order by cb.name, mw.name, co.documentno, col.dateordered, mp.name]]>
	</queryString>

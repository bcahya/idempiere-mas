	<parameter name="M_RMA_ID" class="java.lang.Integer"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select now() as tglskrg,uc.name as ucetak,data.documentno as nodoc,data.created as tgltrx,coalesce(rma.description,'') as descp,bp.value as kdcus,bp.name as nmcust,ad.name as sales,mp.name as nmbrg,
uomconversiontostr(mp.m_product_id,sum(rml.qty)) as qty
from(
select bp.c_bpartner_id as c_bpartner_id,to_char(rm.created,'yyyy-mm-dd') as created,rm.documentno as documentno
from m_rma rm
left join c_bpartner bp on rm.c_bpartner_id=bp.c_bpartner_id
where m_rma_id=$P{M_RMA_ID}) as data
left join M_RMA rma on rma.c_bpartner_id=data.c_bpartner_id
left join m_rmaline rml on rma.m_rma_id=rml.m_rma_id
left join c_bpartner bp on bp.c_bpartner_id=rma.c_bpartner_id
left join m_product mp on mp.m_product_id=rml.m_product_id
left join ad_user ad on ad.ad_user_id=rma.salesrep_id
left join ad_user uc on uc.ad_user_id=$P{AD_User_ID}
where bp.c_bpartner_id=data.c_bpartner_id and to_char(rma.created,'yyyy-mm-dd')=data.created
and rma.docstatus IN('CO','DR')
group by mp.m_product_id,uc.name,rma.description,bp.value,bp.name,ad.name,mp.name,data.documentno,data.created]]>
	</queryString>

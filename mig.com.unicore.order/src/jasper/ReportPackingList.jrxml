	<queryString>
		<![CDATA[SELECT ao.name AS Organisasi, up.datedoc AS TanggalDokumen, up.documentno AS NoDokumen, up.name AS Nama, mw.name AS Gudang,
co.documentno AS NoOrder, ci.documentno AS NoInvoice, cb.name AS Customer, CASE WHEN cbl.name like 'Unde%' THEN '-' ELSE cbl.name END AS Lokasi, mp.name AS Produk,
upl.movementqty AS Kuantitas, cu.name AS UOM, up.tonase AS TON, up.volume AS Volume,
CASE WHEN upc.docaction = 'CO' THEN 'Konfirmasi' WHEN upc.docaction = 'VO' THEN 'Batal' WHEN upc.docaction = 'DR' THEN 'Dalam Proses' END AS StatusKonfirmasi

FROM uns_packinglist_order upo

INNER JOIN ad_org ao ON ao.ad_org_id = upo.ad_org_id

INNER JOIN uns_packinglist up ON up.uns_packinglist_id = upo.uns_packinglist_id

LEFT JOIN uns_packinglist_line upl ON upl.uns_packinglist_order_id = upo.uns_packinglist_order_id

INNER JOIN m_warehouse mw ON mw.m_warehouse_id = upo.m_warehouse_id

LEFT JOIN c_orderline col ON col.c_orderline_id = upl.c_orderline_id

LEFT JOIN c_order co ON co.c_order_id = col.c_order_id

LEFT JOIN c_invoice ci ON ci.c_invoice_id = upo.c_invoice_id

LEFT JOIN c_bpartner cb ON cb.c_bpartner_id = col.c_bpartner_id

LEFT JOIN c_bpartner_location cbl ON cbl.c_bpartner_id = cb.c_bpartner_id

LEFT JOIN m_product mp ON mp.m_product_id = upl.m_product_id

LEFT JOIN c_uom cu ON cu.c_uom_id = mp.c_uom_id

INNER JOIN uns_pl_confirm upc ON upc.uns_packinglist_id = up.uns_packinglist_id

WHERE upc.docaction <> 'CL' AND ao.ad_org_id = $P{AD_Org_ID} AND up.datedoc BETWEEN $P{StartDate} AND $P{EndDate}

ORDER BY up.datedoc, up.documentno, up.name, mw.name]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     coalesce(case
              when (lc.phone is not null) then lc.phone
              when (lc.phone2 is not null) then lc.phone2
              else '?' end ) AS NoTelpon,
     coalesce(uns_employee."name",'-') AS supir,
     coalesce(uns_armada."name",'-') AS kendaraan,
     up."documentno" AS NoPackingList,
     c_bpartner."name" AS NamaCustomer,
     up."datedoc" AS TglPacking,
     c_invoice."documentno" AS NoFak,
     c_invoice."dateinvoiced" AS TglFak,
     c_invoice."grandtotal" As TotFak
FROM
     "adempiere"."uns_packinglist_order" uns_packinglist_order
     LEFT JOIN "adempiere"."uns_packinglist" up ON uns_packinglist_order."uns_packinglist_id" = up."uns_packinglist_id"
     LEFT JOIN "adempiere"."c_invoice" c_invoice ON uns_packinglist_order."c_invoice_id" = c_invoice."c_invoice_id"
     LEFT JOIN "c_bpartner" c_bpartner ON c_invoice."c_bpartner_id" = c_bpartner."c_bpartner_id"
     LEFT JOIN "ad_user" ad_user ON c_bpartner."c_bpartner_id" = ad_user."c_bpartner_id"
     left join "uns_shippingfreight" uns_shippingfreight on up."uns_packinglist_id" = uns_shippingfreight."uns_packinglist_id"
     LEFT JOIN "uns_shipping" uns_shipping ON uns_shipping."uns_shipping_id"=uns_shippingfreight."uns_shipping_id"
     LEFT JOIN "uns_employee" uns_employee ON uns_shipping."uns_employee_id" = uns_employee."uns_employee_id"
     LEFT JOIN "uns_armada" uns_armada ON uns_shipping."uns_armada_id" = uns_armada."uns_armada_id"
     left join "c_bpartner_location" lc on lc."c_bpartner_location_id"=c_invoice."c_bpartner_location_id"
WHERE
     up."documentno" = $P{DocumentNo} AND up."docstatus"='CO']]>
	</queryString>

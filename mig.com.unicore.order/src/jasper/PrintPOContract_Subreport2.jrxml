	<queryString>
		<![CDATA[select now() as tglskrg,u.name as uctk,org.name as org,mr.datedoc as tanggal,mr.documentno as nokontract,cb.name as nmvendor,uomconversiontostr(mp.m_product_id,ml.qty) as qty,mr.description as note,mp.name as nmbrg
from M_Requisition mr
inner join M_RequisitionLine ml on mr.m_requisition_id=ml.m_requisition_id
inner join m_product mp on mp.m_product_id=ml.m_product_id
inner join c_bpartner cb on ml.c_bpartner_id=cb.c_bpartner_id
left join ad_user u on u.ad_user_id=$P{AD_User_ID}
left join ad_org org on org.ad_org_id=mr.ad_org_id
where  mr.M_Requisition_ID=$P{M_Requisition_ID}
and mr.docstatus in('CO','CL')]]>
	</queryString>

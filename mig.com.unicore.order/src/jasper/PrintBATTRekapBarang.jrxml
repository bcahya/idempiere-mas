	<parameter name="UNS_PackingList_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select org.value as organisasi,pl.documentno as nopacking,pl.datedoc as tanggal,now() as tglsekarang, pla.Shipping AS Shipping,
COALESCE(pla.typearmada,'') as TipeKendaraan,
COALESCE(pla.armada, '') as NoKendaraan, COALESCE(pla.nmsupir,'') as Supir,
pl.name as rayon,coalesce(cbp.value,cbp.value,'-') as merk, concat(mp.value,'-',mp.name) as namabarang, uomconversiontostr(mp.m_product_id,sum(plp.confirmedqty)) as qtykirim, COALESCE(mp.Weight,0) AS Weight, COALESCE(mp.Volume,0) AS Volume, coalesce(sum(plp.confirmedqty),0) as qty, data2.salesman as sales, (SELECT SUM(i.GrandTotal) FROM C_Invoice i WHERE EXISTS (SELECT 1 FROM UNS_PackingList_Order lo WHERE lo.C_Invoice_ID = i.C_Invoice_ID AND lo.UNS_PackingList_ID = pl.UNS_PackingList_ID)) AS totalInvoiceOnJuta from(
select pl.UNS_PackingList_ID as pl_id, Array_To_String(Array_Agg(DISTINCT(u.name)), ';') as salesman
from uns_packinglist pl
left join uns_packinglist_order plo on pl.uns_packinglist_id=plo.uns_packinglist_id
left join ad_user u on u.ad_user_id=plo.salesrep_id
where pl.DocStatus NOT IN ('VO', 'RE')
group by pl.UNS_PackingList_ID) data2
inner join uns_packinglist pl on data2.pl_id=pl.uns_packinglist_id
inner join UNS_PL_Confirm plc on pl.uns_packinglist_id=plc.uns_packinglist_id
inner join UNS_PL_ConfirmProduct plp on plp.uns_pl_confirm_id=plc.uns_pl_confirm_id
inner join m_product mp on mp.m_product_id=plp.m_product_id
inner join ad_org org on org.ad_org_id=plc.ad_org_id
LEFT join pl_armada_v pla on pla.uns_packinglist_id=pl.uns_packinglist_id
left join c_bpartner_product bp on bp.m_product_id=mp.m_product_id
left join c_bpartner cbp on bp.c_bpartner_id=cbp.c_bpartner_id
where pl.uns_packinglist_id = $P{UNS_PackingList_ID} and plc.coNfirmtype='PC'
group by org.ad_org_id,pl.uns_packinglist_id,
cbp.c_bpartner_id,mp.m_product_id,data2.salesman,pla.armada,pla.typearmada,pla.nmsupir, pla.Shipping
order by org.value,merk]]>
	</queryString>

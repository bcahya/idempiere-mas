	<parameter name="UNS_Shipping_ID" class="java.lang.Integer"/>
	<parameter name="C_BPartner_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT data.namagudang,jobso.documentno as nojobso,mp.name as nmproduct,uomconversiontostr(mp.m_product_id,col.qtyentered) as qty,sh.documentno as noshipping,sh.datedoc as tglship,
data.noorder as noorder,data.tglorder as tglorder,COALESCE (uat.name, uat.name, '') AS TipeKendaraan,
COALESCE (ua.value, ua.value, '') AS NoKendaraan,sh.driver as supir,cb.name as nmbpartner,
concat_ws(' ',coalesce(cl.address1,''),coalesce(cl.address2,''),coalesce(cl.address3,''),coalesce(cl.address4,''), coalesce(cbl.phone,'')) as al
FROM
(SELECT co.c_order_id as co_id,co.dateordered as tglorder,co.documentno noorder,Array_To_String(Array_Agg(DISTINCT(mw.name)), ';') as namagudang
FROM C_ORDER co
INNER JOIN m_warehouse mw on mw.m_warehouse_id=co.m_warehouse_id
WHERE co.docstatus in('CO','CL')
group by co.c_order_id ) data
INNER JOIN c_orderline col on col.c_order_id=data.co_id
INNER JOIN m_product mp on mp.m_product_id=col.m_product_id
LEFT JOIN C_BPartner_Product mpb on mpb.m_product_id=mp.m_product_id
LEFT JOIN C_BPartner cb on cb.c_bpartner_id=mpb.bpartner_original_id
LEFT JOIN C_BPartner_Location cbl on cbl.c_bpartner_id=cb.c_bpartner_id
left join c_location cl on cl.c_location_id=cbl.c_location_id
INNER JOIN uns_expeditiondetail jobd on jobd.C_Order_id=data.co_id
INNER JOIN uns_expedition jobso on jobso.uns_expedition_id=jobd.uns_expedition_id
INNER JOIN uns_expedition jobs on jobs.jobso_id=jobso.UNS_Expedition_ID
INNER JOIN uns_shippingfreight shf on shf.uns_expedition_id=jobs.uns_expedition_id
INNER JOIN uns_shipping sh on sh.uns_shipping_id=shf.uns_shipping_id
LEFT JOIN uns_armada ua ON ua.uns_armada_id = sh.uns_armada_id
LEFT JOIN uns_armadatype uat ON uat.uns_armadatype_id = ua.uns_armadatype_id
WHERE sh.UNS_shipping_id =$P{UNS_Shipping_ID} AND CASE WHEN sh.UNS_Shipping_ID IS NOT NULL THEN sh.DocStatus NOT IN ('VO', 'RE') ELSE 1=1 END]]>
	</queryString>

	<parameter name="UNS_Shipping_ID" class="java.lang.Integer">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[select
	ship.DocumentNo AS NoShipping,
	org.name as orgname,
	org.description as description,
	location.address1 as address,
	ship.datedoc as datedoc,
	bp.name as partnername,
	bploc.name as partnerloc,
	cd.vouchercode as vouchernumber,
	ship.driver as driver,
	armada.name as carnumber,ship.lastom,
	cd.liters as liters,ds.name,ship.name as desc
from uns_shipping ship
inner join ad_org org on org.ad_org_id = ship.ad_org_id
inner join ad_orginfo orginfo on orginfo.ad_org_id = ship.ad_org_id
inner join uns_charge_detail cd on cd.uns_shipping_id = ship.uns_shipping_id
left join c_bpartner bp on bp.c_bpartner_id = cd.c_bpartner_id
left join c_bpartner_location bploc on bploc.c_bpartner_id = cd.c_bpartner_id
inner join uns_armada armada on armada.uns_armada_id = ship.uns_armada_id
left join c_location location on location.c_location_id = orginfo.c_location_id
left join c_city ds on ds.c_city_id=ship.destination_id
WHERE cd.isfuel = 'Y' and ship.docstatus not in('VO','RE') and ship.Processed = 'Y' AND ship.uns_shipping_id = $P{UNS_Shipping_ID}]]>
	</queryString>

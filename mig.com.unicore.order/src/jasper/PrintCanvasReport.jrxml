	<queryString>
		<![CDATA[select org.name as org,u.name as nmsales,cv.documentno as docno,cv.datedoc tgltrs,cv.description,ccus.name as nmcus,
mp.name as nmbarang,
uomconversiontostr(mp.m_product_id,cvp.qtysold) as qty
from UNS_Cvs_Rpt cv
left join UNS_Cvs_RptCustomer cvc on cv.uns_cvs_rpt_id=cvc.uns_cvs_rpt_id
left join uns_cvscustomer ccus on ccus.uns_cvscustomer_id=cvc.uns_cvscustomer_id
left join UNS_Cvs_RptProduct  cvp on cvp.uns_cvs_rptcustomer_id=cvc.uns_cvs_rptcustomer_id
left join m_product mp on mp.m_product_id=cvp.m_product_id
left join ad_org org on org.ad_org_id=cv.ad_org_id
left join ad_user u on u.ad_user_id=cv.salesrep_id
left join ad_user uu on uu.ad_user_id=$P{AD_User_ID}
where cv.UNS_Cvs_Rpt_ID=$P{UNS_Cvs_Rpt_ID}
order by org.name,cv.datedoc,ccus.name]]>
	</queryString>

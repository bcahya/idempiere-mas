	<parameter name="C_Order_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     ROUND(coalesce(c_orderline."uomqtyenteredl1",'0'),0) AS qty1,
     ROUND(coalesce(c_orderline."uomqtyenteredl2",'0'),0) AS qty2,
     ROUND(coalesce(c_orderline."uomqtyenteredl3",'0'),0) AS qty3,
     ROUND(coalesce(c_orderline."uomqtyenteredl4",'0'),0) AS qty4,
     ROUND(coalesce(c_orderline."uomqtybonusesl1",'0'),0) AS qtyb1,
     ROUND(coalesce(c_orderline."uomqtybonusesl2",'0'),0) AS qtyb2,
     ROUND(coalesce(c_orderline."uomqtybonusesl3",'0'),0) AS qtyb3,
     ROUND(coalesce(c_orderline."uomqtybonusesl4",'0'),0) AS qtyb4,
     case when(c_orderline."isproductbonuses"='Y')then c_orderline."priceactual" ELSE 0 end AS Diskonbrg,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl1_id" ) as Satuan1,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl2_id" ) as satuan2,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl3_id" ) as satuan3,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl4_id" ) as satuan4,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl1_id" ) as Satuanb1,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl2_id" ) as satuanb2,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl3_id" ) as satuanb3,
     ( SELECT "c_uom"."name" FROM "c_uom" WHERE c_uom_id = c_orderline."uomconversionl4_id" ) as satuanb4,
     ( SELECT "ad_user"."name" FROM "ad_user" where ad_user_id= '10001') as user,
     c_order."grandtotal" as Gtot,
     c_order."documentno" AS NoFaktur,
     c_order."dateordered" AS TglFaktur,
     c_bpartner."value" as kd_cust,
     c_bpartner_location."bpartnername" AS NamaToko,
     c_orderline."pricelist" AS Pricelist,
     c_orderline."pricelist"*c_orderline."qtyordered" AS jumlah,
     c_orderline."discountamt" AS Dicount,
     m_product."value" AS KodeProduk,
     m_product."name" AS NmProduk,
     c_ordertax."taxamt" AS ppn,
     emp."value" AS KDSales,
     ad_user."name" AS NamaSales,
     c_bpartner_location."name" AS Alamat,
     c_bpartner_location."phone" as telpon,
     c_bpartner_location."phone" as fax,
     ad_org."name" AS org,
     ad_org."value" as Aorg,
     c_order."documentno" as noorder,
     c_order."description" As Catatan,
     c_order.dateordered +interval '1 days' *(select "c_paymentterm"."netdays" from "c_paymentterm" where c_paymentterm_id=c_order."c_paymentterm_id") as TglJtempo,
     (select name from "c_paymentterm" where c_paymentterm_id=c_order."c_paymentterm_id")  as Carabayar,
     case when(select name from "c_paymentterm" where c_paymentterm_id=c_order."c_paymentterm_id")='TOP 1 Day' then 'CASH' else '' end  as cash,
     getterbilangrupiah(c_order."grandtotal") as terbilang
FROM c_orderline
     INNER JOIN "adempiere"."m_product" m_product ON c_orderline."m_product_id" = m_product."m_product_id"
     INNER JOIN "adempiere"."c_order" c_order ON c_order."c_order_id" = c_orderline."c_order_id"
     INNER JOIN "adempiere"."c_bpartner" c_bpartner ON c_bpartner."c_bpartner_id" = c_order."c_bpartner_id"
     LEFT JOIN "adempiere"."c_ordertax" c_ordertax ON c_order."c_order_id" = c_ordertax."c_order_id"
     LEFT JOIN "adempiere"."ad_user" ad_user ON c_order."salesrep_id" = ad_user."ad_user_id"
     LEFT JOIN "adempiere"."uns_employee" emp ON ad_user."uns_employee_id" = emp."uns_employee_id"
     LEFT JOIN "adempiere"."c_bpartner_location" c_bpartner_location  on c_order."c_bpartner_location_id"=c_bpartner_location."c_bpartner_location_id"
     LEFT JOIN "adempiere"."ad_org" ad_org on c_order."ad_org_id" = ad_org."ad_org_id"
WHERE
     c_order.c_order_id=$P{C_Order_ID}]]>

	<parameter name="M_Movement_ID" class="java.lang.Integer"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     uomconversiontostr(m_movementline."m_product_id",m_movementline."movementqty") as qty,
     m_movement."documentno" AS documentno,
     m_movement."movementdate" AS tanggal,
     m_movement."description" AS catatan,
     m_movement."salesrep_id" AS idsales,
     m_product."name" AS namaproduct,
     m_product."value" AS idproduct,
     loc."value" AS loc,
     locto."value" AS locto,
     ad_user."name" AS sales,
ad_user1."name" AS dibuatoleh
FROM
     "adempiere"."m_movement" m_movement INNER JOIN "adempiere"."m_movementline" m_movementline ON m_movement."m_movement_id" = m_movementline."m_movement_id"
     INNER JOIN "adempiere"."m_product" m_product ON m_movementline."m_product_id" = m_product."m_product_id"
     INNER JOIN "adempiere"."m_locator" m_locator ON m_movementline."m_locator_id" = m_locator."m_locator_id"
INNER JOIN "adempiere"."ad_user" ad_user ON m_movement."salesrep_id" = ad_user."ad_user_id"
INNER JOIN "adempiere"."ad_user" ad_user1 ON ad_user1."ad_user_id"=$P{AD_User_ID}
     LEFT JOIN "adempiere"."c_uom" cu1 ON m_product."uomconversionl1_id"= cu1."c_uom_id"
     LEFT JOIN "adempiere"."c_uom" cu2 ON m_product."uomconversionl2_id"= cu2."c_uom_id"
     LEFT JOIN "adempiere"."c_uom" cu3 ON m_product."uomconversionl3_id"= cu3."c_uom_id"
     LEFT JOIN "adempiere"."c_uom" cu4 ON m_product."uomconversionl4_id"= cu4."c_uom_id"
LEFT JOIN "adempiere"."m_locator" loc ON loc."m_locator_id"=m_movementline."m_locator_id"
LEFT JOIN "adempiere"."m_locator" locto ON locto."m_locator_id"=m_movementline."m_locatorto_id"
WHERE
     m_movement."m_movement_id" = $P{M_Movement_ID}]]>
	</queryString>

	<parameter name="DocumentNo" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT ao.name AS Organisasi, COALESCE (cl.address1, cl.address1, '') AS Alamat, cb.name AS Vendor, cbl.name AS Lokasi, co.dateacct AS TanggalPO, co.documentno AS NoPO, arl.name AS PaymentRule, cpt.name AS CaraPembayaran, mp.value AS Item, co.datepromised AS TanggalPerkiraan,
mp.name AS Description, col.qtyentered AS Qty, col.priceentered AS UnitPrice, cot.taxamt AS Tax, col.linenetamt AS Amount, CASE WHEN cot.taxamt = 0 THEN 'NO' WHEN cot.taxamt <> 0 THEN 'YA' END AS Taxable

FROM c_order co

INNER JOIN ad_org ao ON ao.ad_org_id = co.ad_org_id

INNER JOIN ad_orginfo aoi ON aoi.ad_org_id = ao.ad_org_id

LEFT JOIN c_location cl ON cl.c_location_id = aoi.c_location_id

INNER JOIN c_bpartner cb ON cb.c_bpartner_id = co.c_bpartner_id

INNER JOIN c_bpartner_location cbl ON cbl.c_bpartner_id = cb.c_bpartner_id

INNER JOIN c_paymentterm cpt ON cpt.c_paymentterm_id = co.c_paymentterm_id

INNER JOIN c_orderline col ON col.c_order_id = co.c_order_id

INNER JOIN m_product mp ON mp.m_product_id = col.m_product_id

INNER JOIN c_ordertax cot ON cot.c_order_id = co.c_order_id

INNER JOIN ad_ref_list arl ON arl.value = co.paymentrule

WHERE co.issotrx = 'N' AND arl.ad_reference_id = 195 AND co.documentno = $P{DocumentNo}

ORDER BY ao.ad_org_id]]>
	</queryString>

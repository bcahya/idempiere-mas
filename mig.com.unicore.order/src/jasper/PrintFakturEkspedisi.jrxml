	<queryString>
		<![CDATA[SELECT
ao.name as org,
ci.documentno AS nofk,
cb.name AS customer,
ci.dateacct AS tglfk,
cc.name as charge,
cil.linenetamt AS jumlah,
cil.description AS description,
cbl.name AS lokasi,
now() AS tglskrg,
au.name AS operator,
getterbilangrupiah(cil.linenetamt) AS terbilang

from c_invoice ci

INNER JOIN ad_org ao ON ao.ad_org_id = ci.ad_org_id
INNER JOIN c_bpartner cb ON cb.c_bpartner_id = ci.c_bpartner_id
INNER JOIN c_invoiceline cil ON cil.c_invoice_id = ci.c_invoice_id
INNER JOIN c_charge cc ON cc.c_charge_id = cil.c_charge_id
LEFT JOIN c_bpartner_location cbl ON cbl.c_bpartner_id = cb.c_bpartner_id
INNER JOIN ad_user au on au.ad_user_id = $P{AD_User_ID}

where ci.docstatus in ('CO', 'CL') and ci.c_invoice_id = $P{C_Invoice_ID}]]>
	</queryString>

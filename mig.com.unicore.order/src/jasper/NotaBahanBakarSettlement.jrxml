	<queryString>
		<![CDATA[select
	cr.DocumentNo as NoRS,
	org.name as orgname,
	COALESCE(org.description,'') as description,
	location.address1 as address,
	cr.datetrx as datedoc,
	bp.name as partnername,
	bploc.name as partnerloc,
	cd.vouchercode as vouchernumber,
	drv.Name as driver,
	cd.liters as liters,
	cd.description as desc,
	cd.vehicleno as carnumber
from uns_charge_RS cr
inner join ad_org org on org.ad_org_id = cr.ad_org_id
inner join ad_orginfo orginfo on orginfo.ad_org_id = cr.ad_org_id
inner join uns_charge_detail cd on cd.uns_charge_rs_id = cr.uns_charge_rs_id
inner join c_bpartner drv on drv.c_bpartner_id = cr.c_bpartner_id
left join c_bpartner bp on bp.c_bpartner_id = cd.c_bpartner_id
left join c_bpartner_location bploc on bploc.c_bpartner_id = cd.c_bpartner_id
left join c_location location on location.c_location_id = orginfo.c_location_id
WHERE cd.isfuel = 'Y' and cr.docstatus in('IP', 'CO', 'CL') and cr.uns_charge_rs_id = $P{UNS_Charge_RS_ID}]]>
	</queryString>

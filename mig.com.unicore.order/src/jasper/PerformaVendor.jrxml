	<queryString>
		<![CDATA[select co.documentno as nopo,co.dateordered as tglpo,bpl.bpartnername,vor.nmbrg as nmbrg,mi.documentno as norec,
mi.movementdate as tglrec,
vor.qtypo as qtypo,vor.qtspo as qtspo,vor.qtyrec as qtyrec,vor.qtsrec as qtsrec
from C_Order co
left join c_bpartner bp on bp.c_bpartner_id=co.c_bpartner_id
inner join c_bpartner_location bpl on co.c_bpartner_location_id=bpl.c_bpartner_location_id
inner join c_doctype dt on dt.c_doctype_id=co.c_doctype_id
inner JOIN m_inout mi on mi.c_order_id=co.c_order_id
inner join v_povsreceipt vor on vor.c_order_id=co.c_order_id
where co.dateordered between $P{StartDate} and $P{EndDate} AND
dt.docbasetype='POO' and
co.docstatus in('CO','CL') and mi.docstatus in('CO','CL')
order by co.documentno,co.dateordered,bpl.bpartnername]]>
	</queryString>

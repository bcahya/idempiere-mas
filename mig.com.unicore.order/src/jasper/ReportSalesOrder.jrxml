	<queryString>
		<![CDATA[select ao.name as Departement, date(co.dateordered) as DateOrder, cb.name as BusinessPartner, mw.name as WareHouse, mp.name as Product, col.qtyentered as Qty, cu.name as UOM, col.priceentered as Price, col.linenetamt, col.discount

from c_order co

inner join ad_org ao ON ao.ad_org_id = co.ad_org_id

inner join c_bpartner cb ON cb.c_bpartner_id = co.c_BPartner_id

inner join m_warehouse mw ON mw.m_warehouse_id = co.m_warehouse_id

inner join c_orderline col ON col.c_order_id = co.c_order_id

inner join m_product mp ON mp.m_product_id  = col.m_product_id

inner join c_uom cu ON cu.c_uom_id = col.c_uom_id

where co.docstatus = 'CO' AND co.issotrx = 'Y' AND ao.ad_org_id = $P{AD_Org_ID} AND co.dateordered between $P{StartDate} AND $P{EndDate}

order by co.dateordered, cb.name, mw.name, mp.name]]>
	</queryString>

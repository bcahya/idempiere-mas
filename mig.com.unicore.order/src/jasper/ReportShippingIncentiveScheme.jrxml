	<queryString>
		<![CDATA[SELECT org.Name AS Org, s.DocumentNo AS DocNo, s.Name AS Name, s.ValidFrom AS ValidFrom, rl.Name AS IncentiveType, COALESCE(ry.Name, rll.Value, rlll.Name, ori.Name || '-' || des.Name) AS Criteria, l.IncentiveValue AS IncentiveDriver, l.HelperValue AS HelperValue FROM UNS_ShippingIncentiveSch s

INNER JOIN AD_Org org ON org.AD_Org_ID = s.AD_Org_ID
INNER JOIN UNS_ShippingInctvSch_Line l ON l.UNS_ShippingIncentiveSch_ID = s.UNS_ShippingIncentiveSch_ID
INNER JOIN AD_Ref_List rl ON rl.Value = l.IncentiveType AND rl.AD_Reference_ID = 1000168
LEFT JOIN C_City ori ON ori.C_City_ID = l.Origin_ID
LEFT JOIN C_City des ON des.C_City_ID = l.Destination_ID
LEFT JOIN UNS_Rayon ry ON ry.UNS_Rayon_ID = l.UNS_Rayon_ID
LEFT JOIN AD_Ref_List rll ON rll.Value = l.Reason AND rll.AD_Reference_ID = 1000144
LEFT JOIN AD_Ref_List rlll ON rll.Value = l.Reason AND rlll.AD_Reference_ID = 1000163

WHERE s.DocStatus IN ('CO')

ORDER BY org.Name, DocNo, Criteria]]>
	</queryString>

	<parameter name="UNS_Shipping_ID" class="java.lang.Integer"/>
	<parameter name="UNS_Expedition_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select orgn.name,
concat(lc.address1,' - ',lc.city,' - ',lc.postal) as alamat ,
shp.documentno,
coalesce(emp.name,'') as supir,
coalesce(bp.name,'') as client,
expd.itemdescription as item,
arm.value as platno,
shp.tonase as tonase
from uns_shipping shp
 LEFT JOIN  AD_OrgInfo org on shp.ad_org_id = org.ad_org_id
LEFT JOIN uns_employee emp on shp.uns_employee_id = emp.uns_employee_id
LEFT JOIN uns_armada arm on shp.uns_armada_id = arm.uns_armada_id
LEFT JOIN  UNS_ShippingFreight spf on shp.uns_shipping_id = spf.uns_shipping_id LEFT JOIN  UNS_Expedition exp on spf. UNS_Expedition_id = exp.UNS_Expedition_id LEFT JOIN  UNS_Expeditiondetail expd on exp. UNS_Expedition_id = expd.UNS_Expedition_id
LEFT JOIN c_location lc on org.c_location_id = lc.c_location_id
LEFT JOIN c_bpartner bp on exp.c_bpartner_id = bp.c_bpartner_id
left join ad_org orgn on org.ad_org_id=orgn.ad_org_id
 where shp.docstatus in ('IP','CO','CL') and shp.uns_shipping_id = $P{UNS_Shipping_ID} or exp.UNS_Expedition_ID = $P{UNS_Expedition_ID} and spf.freighttype = 'EXP']]>
	</queryString>

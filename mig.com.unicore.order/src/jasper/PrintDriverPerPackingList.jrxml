	<parameter name="DocumentNo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT pl.documentno as nodcoument,pl.datedoc as tanggal,xp.documentno as joborder,xp.datedoc as tgljob,coalesce(xp.description,'') as Keterangan,
xpjj.documentno as jobspeckno,xpjj.datedoc as tglspek,ar.name as nmarmada,sh.driver,sh.documentno as noShipping
FROM UNS_PackingList as pl
LEFT JOIN UNS_ExpeditionDetail xd on xd.UNS_PackingList_ID=pl.UNS_PackingList_ID
LEFT JOIN UNS_Expedition xp on xp.uns_expedition_id=xd.uns_expedition_id
--INNER JOIN UNS_ExpeditionDetail xpj on xpj.uns_expedition_id=xp.uns_expedition_id
LEFT JOIN UNS_Expedition xpjj on xpjj.jobso_id=xp.uns_expedition_id
LEFT JOIN UNS_ShippingFreight shf on shf.uns_expedition_id=xpjj.uns_expedition_id
LEFT JOIN UNS_Shipping sh on sh.UNS_Shipping_id=shf.uns_shipping_id
LEFT JOIN UNS_Armada ar on sh.uns_armada_id=ar.uns_armada_id
where pl.documentno=$P{DocumentNo}]]>
	</queryString>

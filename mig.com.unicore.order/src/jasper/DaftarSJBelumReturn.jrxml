	<parameter name="C_SalesRegion_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
sr.Name AS Region,
co.documentno AS NoOrder,
pl.documentno AS NoPL,
pr.documentno AS NoPLRT,
io.documentno AS NoSJ,
plc.documentno AS NoPLRTC,
pl.datedoc AS TglKrm,
bp.name AS NmToko,
usr.Name AS User,
bpl.Name AS Location,
pl.Name AS NamePL,
iv.DocumentNo AS NoFk

FROM
UNS_PL_Return pr

inner join uns_packinglist pl on pl.uns_packinglist_id=pr.uns_packinglist_id
inner join UNS_PackingList_Order plo on plo.uns_packinglist_id=pl.uns_packinglist_id
left outer join uns_pl_confirm plc on plc.uns_pl_return_id=pr.uns_pl_return_id
inner join m_inout io on plo.m_inout_id=io.m_inout_id
inner join c_bpartner bp on bp.c_bpartner_id=io.c_bpartner_id
INNER join c_invoice iv on iv.c_invoice_id=io.c_invoice_id
inner join ad_org org on org.ad_org_id=pr.ad_org_id
inner join c_order co on co.c_order_id=plo.c_order_id
INNER JOIN C_BPartner_Location bpl ON bpl.C_BPartner_Location_ID = co.C_BPartner_Location_ID
INNER JOIN AD_User usr ON usr.AD_User_ID = pl.CreatedBy
INNER JOIN C_SalesRegion sr ON sr.C_SalesRegion_ID = org.C_SalesRegion_ID

where pr.docstatus not in ('CO','CL','VO', 'RE') and sr.C_SalesRegion_ID=$P{C_SalesRegion_ID}

order by pl.datedoc, pl.documentno, co.DocumentNo]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     bp."name" AS nama,
     ryn."name" AS rayon,
     bp."c_bpartner_id" AS id,
     ryn."uns_rayon_id" AS idr


FROM
     "c_bpartner" bp
     INNER JOIN c_bpartner_location bpl ON bp."c_bpartner_id" = bpl."c_bpartner_id"
     INNER JOIN uns_rayon ryn ON bpl."uns_rayon_id" = ryn."uns_rayon_id"

WHERE
     bp.C_BPartner_ID = $P{C_BPartner_ID} OR ryn.UNS_Rayon_ID = $P{UNS_Rayon_ID}

ORDER BY bp."name"]]>
	</queryString>

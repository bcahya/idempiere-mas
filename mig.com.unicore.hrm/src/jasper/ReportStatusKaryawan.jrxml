	<queryString>
		<![CDATA[SELECT org.name AS orgname,
emp.Name AS nm_karyawan,
coalesce(jb.name,'') AS jabatan,
CASE WHEN DateContractEnd <= now() THEN 'Expired' WHEN DateContractEnd >= now() THEN 'Active' ELSE 'Not Contract' END,
DateContractEnd AS tgl_kontrak,
reg.C_SalesRegion_ID AS salesregionid,
Reg.Name AS Region,
org.Name AS Organisasi

FROM UNS_Employee emp

LEFT JOIN UNS_Contract_Recommendation cr ON cr.UNS_Contract_Recommendation_ID = emp.UNS_Contract_Recommendation_ID
LEFT JOIN ad_org org ON org.ad_org_id=emp.ad_org_id
LEFT JOIN c_job jb ON jb.c_job_id=emp.c_job_id
LEFT JOIN C_SalesRegion reg ON reg.AD_Org_ID=org.C_SalesRegion_ID

WHERE emp.IsActive='Y' AND (CASE WHEN $P{C_SalesRegion_ID} IS NOT NULL THEN org.C_SalesRegion_ID=$P{C_SalesRegion_ID} ELSE 1=1 END) AND (CASE WHEN $P{UNS_Employee_ID} IS NOT NULL THEN emp.UNS_Employee_ID=$P{UNS_Employee_ID} ELSE 1=1 END) AND
(CASE WHEN $P{Status} = 'ACT' THEN cr.UNS_Contract_Recommendation_ID > 0 AND DateContractEnd >= now() WHEN $P{Status} = 'EXP' THEN cr.UNS_Contract_Recommendation_ID > 0 AND DateContractEnd <= now() WHEN $P{Status} ='NCT' THEN cr.UNS_Contract_Recommendation_ID IS NULL ELSE 1=1 END) AND (CASE WHEN $P{EmploymentType} IS NOT NULL THEN emp.EmploymentType=$P{EmploymentType} ELSE 1=1 END)AND CASE WHEN $P{C_Period_ID} IS NOT NULL THEN DateContractEnd BETWEEN (SELECT StartDate FROM C_Period WHERE C_Period_ID=$P{C_Period_ID}) AND (SELECT EndDate FROM C_Period WHERE C_Period_ID=$P{C_Period_ID}) ELSE 1=1 END AND CASE WHEN $P{AD_Org_ID} IS NOT NULL THEN org.AD_Org_ID=$P{AD_Org_ID} ELSE 1=1 END

GROUP BY salesregionid,emp.UNS_Employee_ID,org.name,jb.c_job_id,DateContractEnd
ORDER BY reg.C_SalesRegion_ID]]>
	</queryString>

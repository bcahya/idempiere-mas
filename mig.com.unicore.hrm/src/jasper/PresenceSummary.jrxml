	<queryString>
		<![CDATA[SELECT c.name as perusahaan, o.name as dept, pr.name as periode, e.value as NIK, e.name, p.name as position,
m.totalSK as sk, m.totalskk as skk, m.totalworkday as wd, m.TotalPayableAbsence as pp, m.TotalAbsence dayoff,
(SELECT SUM(LeaveRequested) FROM UNS_LeavePermissionTrx lp WHERE lp.UNS_Employee_ID=e.UNS_Employee_ID AND lp.C_Period_ID=m.C_Period_ID AND LeaveType='LC') AS lc,
(SELECT SUM(LeaveRequested) FROM UNS_LeavePermissionTrx lp WHERE lp.UNS_Employee_ID=e.UNS_Employee_ID AND lp.C_Period_ID=m.C_Period_ID AND LeaveType='MM') AS m,
(SELECT SUM(LeaveRequested) FROM UNS_LeavePermissionTrx lp WHERE lp.UNS_Employee_ID=e.UNS_Employee_ID AND lp.C_Period_ID=m.C_Period_ID AND LeaveType='PM' AND PermissionType='DI') AS d,
(SELECT SUM(LeaveRequested) FROM UNS_LeavePermissionTrx lp WHERE lp.UNS_Employee_ID=e.UNS_Employee_ID AND lp.C_Period_ID=m.C_Period_ID AND LeaveType='PM' AND PermissionType='ND') AS nd
FROM UNS_Employee e INNER JOIN C_Job p ON e.C_Job_ID = p.C_Job_ID
INNER JOIN UNS_MonthlyPresenceSummary m ON e.UNS_Employee_ID=m.UNS_Employee_ID
INNER JOIN AD_Client c ON c.AD_Client_ID=e.AD_Client_ID
INNER JOIN AD_Org o ON o.AD_ORG_ID=e.AD_ORG_ID
INNER JOIN C_Period pr ON pr.C_Period_ID=m.C_Period_ID
WHERE e.AD_ORG_ID=$P{AD_Org_ID} AND m.C_Period_ID=$P{C_Period_ID}]]>
	</queryString>

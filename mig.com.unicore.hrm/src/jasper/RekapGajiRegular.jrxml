	<parameter name="C_Period_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
e.name AS Nama,
e.value AS NIK,
org.name AS Dept,
(SELECT rl.Name FROM AD_Ref_List rl
	INNER JOIN AD_Reference  ar ON rl.AD_reference_ID = ar.AD_reference_ID
	WHERE rl.value = e.employmentstatus AND ar.Name = '_UNS_EmploymentStatus') AS Status,
period.name AS Periode,
bp.name AS Bagian,
job.Name AS Jabatan,
pe.gpokok AS GajiPokok,
pe.g_t_kesejahteraan AS TunjanganKesejahteraan,
pe.g_t_khusus AS TunjanganKhusus,
pe.g_t_jabatan AS TunjanganJabatan,
pe.gpokok+pe.g_t_kesejahteraan+pe.g_t_khusus+pe.g_t_jabatan AS TGaji,
pe.g_t_lembur AS TunjanganLembur,
pe.a_lemburjampertama AS ALembur,
pe.a_l1+pe.a_l2+pe.a_l3 AS L123,
pe.a_premi AS APremi,
pe.a_rapel AS ARapel,
pe.a_other AS Lain2,
pe.a_jkk+pe.a_jk AS JKKJK,
pe.g_t_lembur+pe.a_lemburjampertama+pe.a_l1+pe.a_l2+pe.a_l3+pe.a_premi+pe.a_rapel+pe.a_other+pe.a_jkk+pe.a_jk AS TLain,
pe.gpokok+pe.g_t_kesejahteraan+pe.g_t_khusus+pe.g_t_jabatan+pe.g_t_lembur+pe.a_lemburjampertama+pe.a_l1+pe.a_l2+pe.a_l3+pe.a_premi+pe.a_rapel+pe.a_other+pe.a_jkk+pe.a_jk AS TotalGajiTunjangan,
pe.p_mangkir AS PMangkir,
pe.p_pinjamankaryawan AS PJKaryawan,
pe.p_label AS PLabel,
pe.p_sptp AS PSPTP,
pe.p_listrikair AS PListrikAir,
pe.p_koperasi AS PKoperasi,
pe.p_obat AS PObat,
pe.p_other AS PLain2,
pe.p_jht AS PJHT,
pe.p_jkk+pe.p_jk AS PJKKJK,
pe.pph21 AS PPPH21,
pe.p_mangkir+pe.p_pinjamankaryawan+pe.p_label+pe.p_sptp+pe.p_listrikair+pe.p_koperasi+pe.p_obat+pe.p_other+pe.p_jht +pe.p_jkk+pe.p_jk+pe.pph21 AS Potongan,
e.UNS_Employee_ID,
e.MaritalStatus,
pe.gpokok+pe.g_t_kesejahteraan+pe.g_t_khusus+pe.g_t_jabatan+pe.g_t_lembur+pe.a_lemburjampertama+pe.a_l1+pe.a_l2+pe.a_l3+pe.a_premi+pe.a_rapel+pe.a_other+pe.a_jkk+pe.a_jk-(pe.p_mangkir+pe.p_pinjamankaryawan+pe.p_label+pe.p_sptp+pe.p_listrikair+pe.p_koperasi+pe.p_obat+pe.p_other+pe.p_jht +pe.p_jkk+pe.p_jk+pe.pph21) AS TotalTerima,
getTerbilangRupiah(pe.gpokok+pe.g_t_kesejahteraan+pe.g_t_khusus+pe.g_t_jabatan+pe.g_t_lembur+pe.a_lemburjampertama+pe.a_l1+pe.a_l2+pe.a_l3+pe.a_premi+pe.a_rapel+pe.a_other+pe.a_jkk+pe.a_jk-(pe.p_mangkir+pe.p_pinjamankaryawan+pe.p_label+pe.p_sptp+pe.p_listrikair+pe.p_koperasi+pe.p_obat+pe.p_other+pe.p_jht +pe.p_jkk+pe.p_jk+pe.pph21))
FROM UNS_Employee e

INNER JOIN AD_Org org ON e.AD_Org_ID = org.AD_Org_ID
INNER JOIN C_BPartner bp ON e.C_BPartner_ID = bp.C_BPartner_ID
INNER JOIN C_Job job ON e.C_Job_ID = job.C_Job_ID
INNER JOIN UNS_Payroll_Employee pe ON e.UNS_Employee_ID = pe.UNS_Employee_ID
INNER JOIN C_Period period ON pe.C_Period_ID = period.C_Period_ID
WHERE e.UNS_Employee_ID = $P{UNS_Employee_ID} AND period.C_Period_ID = $P{C_Period_ID}]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
acl.name as Company,
emp.UNS_Employee_ID as NIK,
emt.name as NamaKaryawan,
rtt.value as NoMesin,
emt.badgenumber as BudgeNumber

FROM
UNS_RegisteredAtt as rtt

INNER JOIN AD_Client acl on acl.AD_Client_ID=rtt.AD_Client_ID
INNER JOIN UNS_EmployeeAtt emt on emt.UNS_RegisteredAtt_ID=rtt.UNS_RegisteredAtt_ID
INNER JOIN UNS_Employee emp on emp.UNS_Employee_ID=rtt.UNS_RegisteredAtt_ID

where rtt.Value = $P{Value}]]>
	</queryString>

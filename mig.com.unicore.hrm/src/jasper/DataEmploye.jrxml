	<queryString>
		<![CDATA[select now() as tglnow,em.value as nik,em.name as nama,job.name as jabatan,
 case when em.idtype='1' THEN 'SI,'
      when em.idtype='2' THEN 'KTP' else 'Pasport' end as idtype,
 em.idno as noidentitas,em.mobilephone as nohp,
 case when em.religion='B' THEN 'Budha'
      when em.religion='C' THEN 'Kristen'
      when em.religion='H' THEN 'Hindu'
      when em.religion='I' Then 'Islam'
      when em.religion='K' then 'Katolik'
      when em.religion='X' then 'free Thinker' else 'Unknown' end as religion,
 case when em.maritalstatus='K0' then 'Kawin 0 Tanggungan'
      when em.maritalstatus='K1' then 'Kawin 1 Tanggungan'
      when em.maritalstatus='K2' then 'Kawin 2 Tanggungan'
      when em.maritalstatus='K3' then 'Kawin 3 Tanggungan'
      when em.maritalstatus='K4' then 'Kawin 4 Tanggungan'
      when em.maritalstatus='K5' then 'Kawin 5 Tanggungan'
      when em.maritalstatus='ND' then 'Not Definition'
      when em.maritalstatus='TK0' then 'Tidak Kawin 0 Tanggungan'
      when em.maritalstatus='TK1' then 'Tidak Kawin 1 Tanggungan' else 'Tidak Kawin 2 Tanggungan' end as marialstatus,
 em.placeofbirth as tempatlahir,em.dateofbirth as tgllahir,em.address as alamat,em.homephone as tlprumah,
 em.employeeinsured1 as anak1,em.employeeinsured2 as anak2,em.employeeinsured3 as anak3,em.hwname as namapasangan,
 arl.name as pendidikan,arlj.name as jurusan,
case when em.gender='F' then 'Wanita' else 'Pria' end as jkelamin
 from UNS_Employee em
 inner join c_job job on em.c_job_id=job.c_job_id
 left join ad_ref_list arl on arl.value=em.highestedbackround and arl.AD_Reference_ID=1000050
 left join ad_ref_list arlj on arlj.value=em.jurusan and arlj.AD_Reference_ID=1000051
 where em.UNS_Employee_ID=$P{UNS_Employee_ID}]]>
	</queryString>

	<parameter name="UNS_Employee_ID" class="java.math.BigDecimal"/>
	<parameter name="C_Period_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[SELECT
dp.presencedate AS tgl,
dp.daytype AS Jns,
e.hourperday AS Norm,
dp.overtime AS Lemb,
dp.ld1 AS LD1,
dp.ld2 AS LD2,
dp.ld3 AS LD3
FROM UNS_Employee e
INNER JOIN AD_Org org ON e.AD_Org_ID = org.AD_Org_ID
INNER JOIN C_BPartner bp ON e.C_BPartner_ID = bp.C_BPartner_ID
INNER JOIN C_Job job ON e.C_Job_ID = job.C_Job_ID
INNER JOIN UNS_YearlyPresenceSummary Yearly ON e.UNS_Employee_ID = Yearly.UNS_Employee_ID
INNER JOIN UNS_MonthlyPresenceSummary Monthly ON Yearly.UNS_YearlyPresenceSummary_ID = Monthly.UNS_YearlyPresenceSummary_ID
INNER JOIN UNS_DailyPresence dp ON Monthly.UNS_MonthlyPresenceSummary_ID = dp.UNS_MonthlyPresenceSummary_ID
INNER JOIN UNS_Payroll_Employee pe ON e.UNS_Employee_ID = pe.UNS_Employee_ID
INNER JOIN C_Period period ON pe.C_Period_ID = period.C_Period_ID
WHERE e.UNS_Employee_ID = $P{UNS_Employee_ID} AND period.c_period_id = $P{C_Period_ID} AND Monthly.C_Period_ID = $P{C_Period_ID}
order by dp.UNS_DailyPresence_ID]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT

e.value AS NIK,
e.Name AS Nama,
org.Name AS Dept,
cj.Name AS Jabatan,
ump.blok_room_no AS NOMess,
(SELECT uu.qty FROM UNS_Utilities_Uses uu
	WHERE mp.value = 'ELC')AS QtyListrik,
(SELECT uu.qty FROM UNS_Utilities_Uses uu
	WHERE mp.value = 'WTR')AS QtyAir,
(SELECT uu.cost FROM UNS_Utilities_Uses uu
	WHERE mp.value = 'ELC') AS CostListrik,
(SELECT uu.cost FROM UNS_Utilities_Uses uu
	WHERE mp.value = 'WTR') AS CostAir

FROM UNS_Utilities_Uses uu
INNER JOIN M_Product mp ON uu.m_product_id = mp.m_product_ID
INNER JOIN UNS_Employee e ON uu.UNS_Employee_ID = e.UNS_Employee_ID
INNER JOIN AD_Org org ON uu.AD_Org_ID = org.AD_Org_ID
INNER JOIN C_Job cj ON e.C_Job_ID = cj.C_Job_ID
INNER JOIN UNS_Mess_Partition ump ON uu.UNS_Mess_Partition_ID = ump.UNS_Mess_Partition_ID]]>
	</queryString>

	<parameter name="UNS_VAT_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT vl.TaxName AS NameClient, REPLACE(REPLACE(vl.TaxSerialNo, '.', ''), '-', '') AS NPWPClient, CASE WHEN vl.IsReplacement = 'Y' THEN 1 ELSE 0 END AS Pengganti, vl.TaxInvoiceNo AS TaxInvoiceNo, DATE(vl.DateInvoiced) AS TANGGAL_FAKTUR, vl.AdditionalReferenceNo AS NOMOR_DOKUMEN_RETUR, DATE(vl.DateTrx) AS TANGGAL_RETUR, DATE_PART('MONTH', vl.DateTrx) AS MASA_PAJAK, DATE_PART('YEAR', vl.DateTrx) AS TAHUN_PAJAK, vl.RevisionAmt AS DPP, vl.RevisionTaxAmt AS PPN

FROM UNS_VAT VAT

INNER JOIN UNS_VATLine vl ON vl.UNS_VAT_ID = vat.UNS_VAT_ID
INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = vl.C_BPartner_ID

WHERE VAT.UNS_VAT_ID = $P{UNS_VAT_ID}

ORDER BY vl.DateInvoiced ASC]]>
	</queryString>

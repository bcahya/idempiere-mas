	<parameter name="Status" class="java.lang.String"/>
	<parameter name="GiroType" class="java.lang.String"/>
	<queryString>
		<![CDATA[select now () tglskrg, sl.name as sales, ad.name as nama,cg.created as tglgiro,(case when cg.disbursementdate is null then cg.duedate else cg.disbursementdate end) as tglcair,cg.duedate as tgljtempo,cba.name as nmbankgirom,coalesce(cb.name,'-') as nmbankpencairan,cb.accountno as norek,cg.name as nogiro,cg.limitamt as rpgiro,cp.name as nm_customer,
rf.value as status,rf.name as kstatus,'Masukan' as keterangan
from uns_customerbg as cg
inner join uns_customerbg_action cga on cga.uns_customerbg_id=cg.uns_customerbg_id
left join c_bank cba on cba.c_bank_id = cg.c_bank_id
left join c_bpartner as cp on cp.c_bpartner_id=cg.c_bpartner_id
left join c_bankaccount cb on cb.c_bankaccount_id=cga.c_bankaccount_id
left join ad_user ad on ad.ad_user_id=$P{AD_User_ID}
left join ad_user sl on sl.ad_user_id = cg.createdby
left join ad_ref_list rf on rf.value= cga.action and rf.ad_reference_id = (select ar.ad_reference_id from ad_reference ar where ar.name='_Giro Action List')
WHERE CASE WHEN cga.action IN ($P{Status}) THEN cga.action = $P{Status} ELSE 1=1 END and cg.created between $P{DateFrom} AND $P{DateTo}
and $P{GiroType}='GM']]>
	</queryString>

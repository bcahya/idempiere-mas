	<queryString>
		<![CDATA[SELECT
	org.Name AS Org,
	emp.Name AS employee,
	el.documentno AS Docno,
	el.loanamt AS pinjaman,
	el.installment AS angsuran,
	el.Reason AS keterangan,
	el.installmentPeriod AS tenor,
	el.trxdate AS date,
	orgi.phone AS phone,
	now(),
	getterbilangrupiah(el.loanamt) AS terbilang,
	getsimpleaddress(orgi.C_Location_ID) AS orgadress

FROM UNS_Employee_Loan el

INNER JOIN AD_Org org ON org.AD_Org_ID=el.AD_Org_ID
INNER JOIN UNS_Employee emp ON emp.UNS_Employee_ID=el.UNS_Employee_ID
INNER JOIN AD_OrgInfo orgi ON orgi.AD_Org_ID=org.AD_Org_ID

Where el.DocStatus IN ('CO','CL') AND $P{UNS_Employee_Loan_ID}=UNS_Employee_Loan_ID]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT tbc.DocumentNo AS DocNo, tbc.DateRequired AS DateRequired, tbc.DateConfirm AS DateConfirm, orgto.Name AS OrgTo, orgfrom.Name AS OrgFrom,
accfrom.Name AS AccountFrom, accto.Name AS AccountTo, tbc.AmountRequested AS AmountRequested, tbc.AmountConfirmed AS AmountConfirmed, COALESCE(tbc.Description, '-') AS Description,
bsl.StatementLineDate AS DateStatement, COALESCE(cp.DocumentNo, cc.Name) AS PaymentCharge, bsl.StmtAmt AS Amount, COALESCE(bsl.Description, '-') AS Keterangan, au.Name AS PrintedBy, now() AS DateNow

FROM UNS_TransferBalance_Confirm tbc

INNER JOIN AD_Org orgto ON orgto.AD_Org_ID = tbc.AD_Org_ID

INNER JOIN AD_Org orgfrom ON orgfrom.AD_Org_ID = tbc.AD_OrgFrom_ID

INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = tbc.C_BPartner_ID

INNER JOIN UNS_TransferBalance_Request tbr ON tbr.UNS_TransferBalance_Request_ID = tbc.UNS_TransferBalance_Request_ID

INNER JOIN C_BankStatementLine bsl ON bsl.UNS_TransferBalance_Request_ID = tbr.UNS_TransferBalance_Request_ID

INNER JOIN C_BankAccount accfrom ON accfrom.C_BankAccount_ID = tbc.AccountFrom_ID

INNER JOIN C_BankAccount accto ON accto.C_BankAccount_ID = tbc.AccountTo_ID

LEFT JOIN C_Charge cc ON cc.C_Charge_ID = bsl.C_Charge_ID

LEFT JOIN C_Payment cp ON cp.C_Payment_ID = bsl.C_Payment_ID

INNER JOIN AD_User au ON au.AD_User_ID = $P{AD_User_ID}

WHERE tbc.UNS_TransferBalance_Confirm_ID = $P{UNS_TransferBalance_Confirm_ID} AND tbc.DocStatus <> 'DR']]>
	</queryString>

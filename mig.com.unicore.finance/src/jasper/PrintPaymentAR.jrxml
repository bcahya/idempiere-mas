	<parameter name="C_Payment_ID" class="java.lang.Integer"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
 case when cl.bpartnername=null then bp.name else cl.bpartnername end as vendor,
 p.documentno as documentno,sal.name as nm_sales,
 p.datetrx as datetrx,
 coalesce(p.description,' ') as description,
 coalesce(i.description,' ') as idescp,
 i.documentno as referenceno,
 i.dateinvoiced as dateinvoiced,bc.name,
 i.dateinvoiced + INTERVAL '1 DAYS' * pt.netdays as duedate,
 i.grandtotal as grandtotal,
 p.chargeamt as charge,
 ch.name as nmcharge,
 pa.invoiceamt as invoiceopen,
 pa.paytooverunderamount as payamt,
 pa.description as allocatedescription,
now() as tanggal,u.name as Nmuser
FROM
 c_paymentallocate pa
INNER JOIN  c_invoice i ON i.c_invoice_id = pa.c_invoice_id
INNER JOIN  c_paymentterm pt on pt.c_paymentterm_id = i.c_paymentterm_id
INNER JOIN  c_payment p ON p.c_payment_id = pa.c_payment_id
INNER JOIN  c_bpartner bp ON bp.c_bpartner_id = p.c_bpartner_id
LEFT JOIN c_bpartner_location cl on cl.c_bpartner_location_id =i.c_bpartner_location_id
left join c_charge ch on ch.c_charge_id=p.c_charge_id
left join c_bankaccount bc on bc.c_bankaccount_id=p.c_bankaccount_id
left join ad_user sal on sal.ad_user_id=i.salesrep_id
LEFT join ad_user u on u.ad_user_id = $P{AD_User_ID}
WHERE p.c_payment_id = $P{C_Payment_ID}
and p.c_order_id is null
and p.c_doctype_id in(select c_doctype_id from c_doctype  where docbasetype='ARR')
union all
SELECT
 case when cl.bpartnername=null then bp.name else cl.bpartnername end as vendor,
 p.documentno as documentno,sal.name as nm_sales,
 p.datetrx as datetrx,
 coalesce(p.description,' ') as description,
 coalesce(i.description,' ') as idescp,
 i.documentno as referenceno,
 i.dateordered  as dateinvoiced,bc.name,
 i.dateordered + INTERVAL '1 DAYS' * pt.netdays as duedate,
 i.grandtotal as grandtotal,
 p.chargeamt as charge,
 ch.name as nmcharge,
 pa.invoiceamt as invoiceopen,
 pa.paytooverunderamount as payamt,
 pa.description as allocatedescription,
now() as tanggal,u.name as Nmuser
FROM
 c_paymentallocate pa
INNER JOIN  c_order i ON i.c_order_id = pa.c_order_id
INNER JOIN  c_paymentterm pt on pt.c_paymentterm_id = i.c_paymentterm_id
INNER JOIN  c_payment p ON p.c_payment_id = pa.c_payment_id
INNER JOIN  c_bpartner bp ON bp.c_bpartner_id = p.c_bpartner_id
LEFT JOIN c_bpartner_location cl on cl.c_bpartner_location_id =i.c_bpartner_location_id
left join c_charge ch on ch.c_charge_id=p.c_charge_id
left join c_bankaccount bc on bc.c_bankaccount_id=p.c_bankaccount_id
left join ad_user sal on sal.ad_user_id=i.salesrep_id
LEFT join ad_user u on u.ad_user_id = $P{AD_User_ID}
WHERE p.c_payment_id =$P{C_Payment_ID}
and pa.c_invoice_id is null
and p.c_doctype_id in(select c_doctype_id from c_doctype  where docbasetype='ARR')]]>

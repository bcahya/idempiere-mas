	<queryString>
		<![CDATA[SELECT ao.name AS Departement, aot.name AS InterOrganization, cba.name AS FromAccount, cbat.name AS AccountTo, utr.documentno AS DocumentNO, utr.daterequired AS DateRequired, cb.name AS BusinessPartner, upcr.amountrequested AS AmountRequested, upcr.amountconfirmed AS AmountConfirmed, utr.prevbalanceamt AS PreviousBalance

FROM uns_transferbalance_request utr

INNER JOIN ad_org ao ON ao.ad_org_id = utr.ad_org_id

INNER JOIN ad_org aot ON aot.ad_org_id = utr.ad_orgto_id

INNER JOIN c_bankaccount cba ON cba.c_bankaccount_id = utr.accountfrom_id

INNER JOIN c_bankaccount cbat ON cbat.c_bankaccount_id = utr.accountto_id

INNER JOIN c_bpartner cb ON cb.c_bpartner_id = utr.c_bpartner_id

INNER JOIN uns_pettycashrequest upcr ON upcr.uns_transferbalance_request_id = utr.uns_transferbalance_request_id

WHERE utr.docstatus = 'CO' AND utr.daterequired BETWEEN $P{StartDate} AND $P{EndDate}

ORDER BY aot.name, cba.name, cbat.name, utr.documentno]]>
	</queryString>

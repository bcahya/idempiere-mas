	<queryString>
		<![CDATA[SELECT now() as tglskrg,ad.name as nama,gk.uns_create_cheque_date as tglgiro,gk.uns_cheque_disbursementdate as tglcair,gk.uns_cheque_disbursementdate as tgljtempo,cb.name as nmbank,cb.accountno as norek, chequeno as nogiro,p.totalamt as rpgiro,bp.name as nm_customer,rf.value as status,rf.name as kstatus,'Keluaran' as keterangan
from uns_cheque_reconciliation as gk
LEFT join c_payment p on p.c_payment_id=gk.c_payment_id
LEFT JOIN c_bpartner bp on bp.c_bpartner_id=p.c_bpartner_id
left join uns_chequebook cq on cq.uns_chequebook_id=gk.uns_chequebook_id
left join c_bankaccount cb on cb.c_bankaccount_id=cq.c_bankaccount_id
left join ad_user ad on ad.ad_user_id=$P{AD_User_ID}
left join ad_ref_list rf on rf.value=gk.status and rf.ad_reference_id = (select ar.ad_reference_id from ad_reference ar where ar.name='_UNS_ChequeStatus')
where CASE WHEN gk.status IN ($P{Status}) THEN gk.status = $P{Status} ELSE 1=1 END AND gk.uns_create_cheque_date between $P{DateFrom} AND $P{DateTo}]]>
	</queryString>

	<parameter name="C_SalesRegion_ID" class="java.lang.Integer"/>
	<parameter name="C_Charge_ID" class="java.lang.Integer"/>
	<parameter name="C_BankAccount_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select org.name as nmorg,cg.name as nmcharge, sum(amountap) as amountap, sum(amountar) as amountar,
sr.name,cb.name as nmbank
from chargestatement_v cv
inner join c_charge cg on cg.c_charge_id=cv.charge
inner join ad_org org on org.ad_org_id=cv.aorg
left join c_salesregion sr on sr.c_salesregion_id=$P{C_SalesRegion_ID}
left join ad_user u on u.ad_user_id =$P{AD_User_ID}
inner join c_bankaccount cb on cb.c_bankaccount_id=cv.c_bankaccount_id
where cv.dacc between $P{StartDate} and $P{EndDate}
and (case when $P{C_Charge_ID}!=0 or $P{C_Charge_ID} is not null then cg.C_Charge_ID=$P{C_Charge_ID} else 1=1 end)
and cv.aorg in(select ad_org_id from ad_org where c_salesregion_id=$P{C_SalesRegion_ID})
and (case when $P{C_BankAccount_ID}!=0 or $P{C_BankAccount_ID} is not null then cb.C_BankAccount_ID=$P{C_BankAccount_ID} else 1=1 end)
group by cg.c_charge_id,org.name,sr.c_salesregion_id,cv.C_BankAccount_ID,cb.name
order by org.name]]>
	</queryString>

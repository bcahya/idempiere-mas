	<queryString>
		<![CDATA[Select
v.documentno as vatdocno,
v.datereport as daterpt,
org.name as org,
bpt.name as bpartner,
i.documentno as invoice,
vline.taxinvoiceno as taxnoinv,
vline.revisiontaxamt as revisitaxamt,
vline.revisionbeforetaxamt as revisiamt,
vline.taxname as taxname

from UNS_VAT V
inner join UNS_VATLine vline ON v.UNS_VAT_ID=vline.UNS_VAT_ID
inner join AD_Org org ON org.AD_Org_ID=v.AD_Org_ID
inner join C_Invoice i ON i.C_Invoice_ID=vline.C_Invoice_ID
inner join C_BPartner bpt ON bpt.C_Bpartner_ID=vline.C_BPartner_ID
LEFT JOIN UNS_VATPayment vp ON vp.UNS_VatPayment_ID=v.UNS_VatPayment_ID


where v.datereport between $P{DateFrom} and $P{DateTo} AND v.UNS_VATPayment_ID is null]]>
	</queryString>

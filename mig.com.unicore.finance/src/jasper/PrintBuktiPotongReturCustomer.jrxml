	<parameter name="C_Payment_ID" class="java.lang.Integer"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select now() as tglcetak,bl.bpartnername as namacus,pm.documentno as nopayment,pm.datetrx as tgl_payment,pal.amount as nilai,adu.name as nm_sales,
pal.invoiceamt as tofaktur,(SELECT documentno from c_invoice a where a.c_invoice_id=(SELECT b.c_invoice_id from c_invoice b where b.c_invoice_id=pal.c_invoice_id)) AS nofak,
(SELECT dateinvoiced from c_invoice a where a.c_invoice_id=(SELECT b.c_invoice_id from c_invoice b where b.c_invoice_id=pal.c_invoice_id)) AS tglfak,
invoiceopen(pal."c_invoice_id",0) As Sisa,
(select a.dateinvoiced +interval '1 days' *(select "c_paymentterm"."netdays" from "c_paymentterm" where c_paymentterm_id=a."c_paymentterm_id") from c_invoice a where a.c_invoice_id=(SELECT b.c_invoice_id from c_invoice b where b.c_invoice_id=pal.c_invoice_id))  as TglJtempo,
(SELECT coalesce(a.grandtotal, 0) from c_invoice a where a.cn_invoice_id=(SELECT b.c_invoice_id from c_invoice b where b.c_invoice_id=pal.c_invoice_id)) AS retur,
(SELECT documentno from c_invoice a where a.cn_invoice_id=(SELECT b.c_invoice_id from c_invoice b where b.c_invoice_id=pal.c_invoice_id)) AS noretur,u.name as ucetak
from c_payment pm
left join c_bpartner as bp on bp.c_bpartner_id=pm.c_bpartner_id
left join c_bpartner_location bl on bl.c_bpartner_id=bp.c_bpartner_id
left join c_paymentallocate pal on pal.c_payment_id=pm.c_payment_id
inner join c_invoice as inv on inv.cn_invoice_id=pal.c_invoice_id
left join ad_user adu on inv.salesrep_id=adu.ad_user_id
left join ad_user u on u.ad_user_id = $P{AD_User_ID}
where pm.c_payment_id=$P{C_Payment_ID}
and pm.docstatus='CO']]>
	</queryString>

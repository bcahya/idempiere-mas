	<parameter name="C_SalesRegion_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT org.name,trf.documentno,trf.datedoc,trf.description,orgv.name as orginvoice,ci.documentno,bl.bpartnername,ci.dateinvoiced,rf.name as keterangan,bli.amounttrf,ci.grandtotal as netinvoice,vat.c_invoice_id,cb.value as nmbank
 FROM UNS_BillingTrf trf
 INNER JOIN UNS_BillingTrfValidation trfv on trfv.uns_billingtrf_id=trf.uns_billingtrf_id
 INNER JOIN UNS_BillingTrf_Detail trfd on trfd.uns_billingtrfvalidation_id=trfv.uns_billingtrfvalidation_id
 INNER JOIN ad_org orgv on orgv.ad_org_id=trfv.ad_org_id
 INNER JOIN c_salesregion so on so.c_salesregion_id=orgv.c_salesregion_id
 INNER JOIN UNS_BillingTrf_Invoice bli on trfd.uns_billingtrf_detail_id=bli.uns_billingtrf_detail_id
 INNER JOIN c_invoice ci on ci.c_invoice_id=bli.c_invoice_id
 INNER JOIN c_bpartner_location bl on bl.c_bpartner_location_id=ci.c_bpartner_location_id
 INNER JOIN C_Payment cp on cp.c_payment_id=trfd.c_payment_id
 INNER JOIN ad_ref_list rf on rf.value=cp.tendertype and rf.ad_reference_id=214
 INNER JOIN ad_org org on org.ad_org_id=trf.ad_org_id
 INNER JOIN C_BankAccount cb on trfv.AccountTo_ID=cb.c_bankAccount_ID
 LEFT JOIN UNS_VATLine vat on ci.c_invoice_id=vat.c_invoice_id
 WHERE trf.docstatus in('CO','CL') AND trf.datedoc between $P{StartDate} and $P{EndDate}
AND CASE WHEN $P{C_SalesRegion_ID}!=0 THEN so.C_Salesregion_ID=$P{C_SalesRegion_ID} ELSE 1=1 END
order by trf.datedoc,orgv.name,ci.documentno]]>
	</queryString>

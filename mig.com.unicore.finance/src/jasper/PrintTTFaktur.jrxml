	<queryString>
		<![CDATA[SELECT b.DocumentNo AS DocumentNo, bp.Name AS NamaCustomer, bpl.BPartnerName AS Lokasi, iv.DocumentNo AS NoFaktur, bl.DateInvoiced AS TglFaktur, bl.NetAmtToInvoice AS JmlFaktur, bg.DateDoc AS Tanggal, bl.DueDate AS DueDate

FROM UNS_Billing b

INNER JOIN UNS_BillingGroup bg ON bg.UNS_BillingGroup_ID = b.UNS_BillingGroup_ID
INNER JOIN UNS_BillingLine bl ON bl.UNS_Billing_ID = b.UNS_Billing_ID
INNER JOIN C_Invoice iv ON iv.C_Invoice_ID = bl.C_Invoice_ID
INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = b.C_BPartner_ID
INNER JOIN C_BPartner_Location bpl ON bpl.C_BPartner_Location_ID = iv.C_BPartner_Location_ID

WHERE b.UNS_Billing_ID = $P{UNS_Billing_ID}]]>
	</queryString>

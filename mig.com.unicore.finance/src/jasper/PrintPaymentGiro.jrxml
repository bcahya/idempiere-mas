	<queryString>
		<![CDATA[select now() tglskrg,u.name as nmuser,cb.receiptdate as tgltrx,cb.name as nogiro,
gac.documentno as nodoc,
case when ci.documentno is null then (select c_invoice.documentno from c_invoice where c_invoice.c_order_id=cbl.c_order_id) else ci.documentno  end as nofaktur,
case when ci.documentno is null then (select c_bpartner.name from c_bpartner where c_bpartner_id in(select c_invoice.c_bpartner_id from c_invoice  where c_invoice.c_order_id=cbl.c_order_id)) else ci.documentno  end as name,
case when ci.documentno is null then (select c_invoice.grandtotal from c_invoice where c_invoice.c_order_id=cbl.c_order_id) else ci.grandtotal  end as gttot,
cbl.paidamt as tgiro
from UNS_CustomerBG cb
left join uns_customerbg_invlist cbl on cb.uns_customerbg_id=cbl.uns_customerbg_id
left join UNS_CustomerBG_Action gac on gac.uns_customerbg_id=cb.uns_customerbg_id
left join c_bpartner cp on cb.c_bpartner_id=cp.c_bpartner_id
left join c_invoice ci on ci.c_invoice_id=cbl.c_invoice_id
left join ad_user u on u.ad_user_id=$P{AD_User_ID}
where cb.receiptdate>=$P{StartDate} and cb.receiptdate<=$P{EndDate}
and cb.createdby=$P{AD_User_ID}]]>
	</queryString>

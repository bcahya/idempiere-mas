	</parameter>
	<parameter name="EndDate" class="java.sql.Timestamp">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT ao.name AS Departement, utr.daterequired AS DateRequired, aot.name AS RequestOrganization, cb.name AS BusinessPartner, cbat.name AS AccountTo,

COALESCE (cbaf.name, cbaf.name, '-') AS AccountFrom,

utc.documentno AS DocumentNo, utc.daterequired AS DateRequired, utc.dateconfirm AS DateConfirm,

case when utc.docstatus = 'CO' THEN 'Di Setujui' WHEN utc.docstatus = 'VO' THEN 'Di Batalkan' ELSE 'Belum Di Konfirmasi' END AS Status, utc.amountrequested AS AmountRequested,

CASE WHEN utc.docstatus = 'CO' OR utc.docstatus = 'DR' THEN utc.amountconfirmed WHEN utc.docstatus = 'VO' THEN '0' END AS AmountConfirmed

FROM uns_transferbalance_confirm utc

INNER JOIN ad_org ao ON ao.ad_org_id = utc.ad_org_id

INNER JOIN uns_transferbalance_request utr ON utr.uns_transferbalance_request_id = utc.uns_transferbalance_request_id

LEFT JOIN ad_org aot ON aot.ad_org_id = utr.ad_orgto_id

LEFT JOIN c_bpartner cb ON cb.c_bpartner_id = utc.c_bpartner_id

LEFT JOIN c_bankaccount cbat ON cbat.c_bankaccount_id = utr.accountto_id

LEFT JOIN c_bankaccount cbaf ON cbaf.c_bankaccount_id = utr.accountfrom_id

WHERE ao.ad_org_id = $P{AD_Org_ID} AND utc.dateconfirm BETWEEN $P{StartDate} AND $P{EndDate}

ORDER BY aot.name, Status, cbat.name]]>
	</queryString>

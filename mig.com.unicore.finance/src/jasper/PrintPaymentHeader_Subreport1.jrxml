	<queryString>
		<![CDATA[select co.documentno as nodoc,co.dateordered as tglorder
from c_payment cp
inner join c_order co on cp.c_bpartner_id=co.c_bpartner_id and co.paymentrule='B'
left join m_inout mi on co.c_order_id=mi.c_order_id

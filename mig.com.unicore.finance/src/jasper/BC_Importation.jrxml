	</parameter>
	<queryString>
		<![CDATA[SELECT bcc.value as bccode, bc.bcno, bc.bcdocdate, io.documentno, bc.movementdate,
p.value, p.name, bc.hscode, bc.importdutypersent, bc.qtyinvoiced, uom.uomsymbol,
cu.iso_code as valas, bc.cifamt, bc.linenetamt, bc.currencyrate, bc.brutoquantity,
bp.name as supplier, c.name as perusahaan FROM uns_bc_in_v bc
INNER JOIN UNS_BCCode bcc ON bc.UNS_BCCode_ID=bcc.UNS_BCCode_ID
LEFT JOIN M_InOut io ON bc.M_InOut_ID=io.M_InOut_ID
INNER JOIN C_BPartner bp ON bc.C_BPartner_ID=bp.C_BPartner_ID
INNER JOIN M_Product p ON bc.M_Product_ID=p.M_Product_ID
INNER JOIN C_UOM uom ON uom.C_UOM_ID=bc.C_UOM_ID
INNER JOIN C_Currency cu ON cu.C_Currency_ID=bc.C_Currency_ID
INNER JOIN AD_Client c ON c.AD_Client_ID=bc.AD_Client_ID
WHERE bc.UNS_BCCode_ID=$P{UNS_BCCode_ID} AND bc.bcdocdate BETWEEN $P{DateFrom} AND $P{DateTo}
ORDER BY bc.bcdocdate, bc.bcno, p.value]]>
	</queryString>

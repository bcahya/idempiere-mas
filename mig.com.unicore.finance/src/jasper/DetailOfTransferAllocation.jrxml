	<queryString>
		<![CDATA[SELECT * FROM (SELECT btv.UNS_BillingTrfValidation_ID AS btvID, btv.DocumentNo AS DocNo, btv.DateAcct AS DateAcct, bafrom.Name AS AccountFrom, bato.Name AS AccountTo, btv.AmountTrf AS AmountTrf, org.Value AS Org, CONCAT(bp.Value, '-', bp.Name) AS Partner, iv.DocumentNo AS Invoice, iv.GrandTotal AS GrandTotal FROM UNS_BillingTrfValidation btv

INNER JOIN UNS_BillingTrf_Detail btd ON btd.UNS_BillingTrfValidation_ID = btv.UNS_BillingTrfValidation_ID
INNER JOIN C_BankAccount bafrom ON bafrom.C_BankAccount_ID = btv.AccountFrom_ID
INNER JOIN C_BankAccount bato ON bato.C_BankAccount_ID = btv.AccountTo_ID
INNER JOIN UNS_PaymentReceipt pr ON pr.UNS_PaymentReceipt_ID = btd.UNS_PaymentReceipt_ID
INNER JOIN UNS_BillingGroup_Result bgr ON bgr.UNS_BillingGroup_Result_ID = pr.UNS_BillingGroup_Result_ID
INNER JOIN UNS_Billing_Result br ON br.UNS_BillingGroup_Result_ID = bgr.UNS_BillingGroup_Result_ID
INNER JOIN UNS_BillingLine_Result blr ON blr.UNS_Billing_Result_ID = br.UNS_Billing_Result_ID AND blr.AD_Org_ID = pr.AD_Org_ID
INNER JOIN C_Invoice iv ON iv.C_Invoice_ID = blr.C_Invoice_ID
INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = iv.C_BPartner_ID
INNER JOIN AD_Org org ON org.AD_Org_ID = iv.AD_Org_ID

WHERE btv.DocStatus IN ('CO', 'CL') AND btv.DateAcct BETWEEN $P{DateFrom} AND $P{DateTo} AND (CASE WHEN $P{UseBankFrom} = 'Y' AND $P{C_BankAccount_ID} IS NOT NULL THEN bafrom.C_BankAccount_ID = $P{C_BankAccount_ID} WHEN $P{UseBankFrom} = 'N' AND $P{C_BankAccount_ID} IS NOT NULL THEN bato.C_BankAccount_ID = $P{C_BankAccount_ID} ELSE 1=1 END)

UNION ALL

SELECT btv.UNS_BillingTrfValidation_ID AS btvID, btv.DocumentNo AS DocNo, btv.DateAcct AS DateAcct, bafrom.Name AS AccountFrom, bato.Name AS AccountTo, btv.AmountTrf AS AmountTrf, org.Value AS Org, CONCAT(bp.Value, '-', bp.Name) AS Partner, iv.DocumentNo AS Invoice, iv.GrandTotal AS GrandTotal FROM UNS_BillingTrfValidation btv

INNER JOIN UNS_BillingTrf_Detail btd ON btd.UNS_BillingTrfValidation_ID = btv.UNS_BillingTrfValidation_ID
INNER JOIN C_BankAccount bafrom ON bafrom.C_BankAccount_ID = btv.AccountFrom_ID
INNER JOIN C_BankAccount bato ON bato.C_BankAccount_ID = btv.AccountTo_ID
INNER JOIN C_PaymentAllocate pa ON pa.C_Payment_ID = btd.C_Payment_ID
INNER JOIN C_Invoice iv ON iv.C_Invoice_ID = pa.C_Invoice_ID
INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = iv.C_BPartner_ID
INNER JOIN AD_Org org ON org.AD_Org_ID = iv.AD_Org_ID

WHERE btv.DocStatus IN ('CO', 'CL') AND btv.DateAcct BETWEEN $P{DateFrom} AND $P{DateTo} AND (CASE WHEN $P{UseBankFrom} = 'Y' AND $P{C_BankAccount_ID} IS NOT NULL THEN bafrom.C_BankAccount_ID = $P{C_BankAccount_ID} WHEN $P{UseBankFrom} = 'N' AND $P{C_BankAccount_ID} IS NOT NULL THEN bato.C_BankAccount_ID = $P{C_BankAccount_ID} ELSE 1=1 END)

UNION ALL

SELECT btv.UNS_BillingTrfValidation_ID AS btvID, btv.DocumentNo AS DocNo, btv.DateAcct AS DateAcct, bafrom.Name AS AccountFrom, bato.Name AS AccountTo, btv.AmountTrf AS AmountTrf, org.Value AS Org, CONCAT(bp.Value, '-', bp.Name) AS Partner, iv.DocumentNo AS Invoice, iv.GrandTotal AS GrandTotal FROM UNS_BillingTrfValidation btv

INNER JOIN UNS_BillingTrf_Detail btd ON btd.UNS_BillingTrfValidation_ID = btv.UNS_BillingTrfValidation_ID
INNER JOIN C_BankAccount bafrom ON bafrom.C_BankAccount_ID = btv.AccountFrom_ID
INNER JOIN C_BankAccount bato ON bato.C_BankAccount_ID = btv.AccountTo_ID
INNER JOIN UNS_BillingTrfValidation btvv ON btvv.Reference_ID = btd.C_Payment_ID
INNER JOIN UNS_BillingTrf_Detail btdd ON btdd.UNS_BillingTrfValidation_ID = btvv.UNS_BillingTrfValidation_ID
INNER JOIN C_Payment p ON p.C_Payment_ID = btdd.C_Payment_ID
INNER JOIN C_PaymentAllocate paa ON paa.C_Payment_ID = p.C_Payment_ID
INNER JOIN C_Invoice iv ON iv.C_Invoice_ID = paa.C_Invoice_ID
INNER JOIN C_BPartner bp ON bp.C_BPartner_ID = iv.C_BPartner_ID
INNER JOIN AD_Org org ON org.AD_Org_ID = iv.AD_Org_ID

WHERE btv.DocStatus IN ('CO', 'CL') AND btv.DateAcct BETWEEN $P{DateFrom} AND $P{DateTo} AND (CASE WHEN $P{UseBankFrom} = 'Y' AND $P{C_BankAccount_ID} IS NOT NULL THEN bafrom.C_BankAccount_ID = $P{C_BankAccount_ID} WHEN $P{UseBankFrom} = 'N' AND $P{C_BankAccount_ID} IS NOT NULL THEN bato.C_BankAccount_ID = $P{C_BankAccount_ID} ELSE 1=1 END)
) AS Master

ORDER BY Master.DateAcct, Master.DocNo, Master.AccountFrom, Master.AccountTo]]>

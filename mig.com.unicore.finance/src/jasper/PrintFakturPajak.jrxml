	<parameter name="UNS_VATDocInOut_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT ao.name AS Organisasi, COALESCE (cl.address1, '-') AS LokasiOrganisasi, cb.name AS Customer, clb.address1 AS LokasiCustomer, mp.value AS Key, mp.name AS NamaBarang, cil.qtyinvoiced AS Qty, cu.name AS Satuan, cil.linenetamt AS Harga, ci.totallines AS Total, it.TaxAmt AS PPN, NOW() AS TanggalHariIni, concat (uvr.prefixno, '-', uvt.sequenceusedno) AS noVAT, ci.C_Invoice_ID AS Invoice

FROM UNS_VATDocInOut uv

INNER JOIN C_Invoice ci ON uv.c_invoice_id = ci.c_invoice_id

INNER JOIN C_InvoiceTax it ON it.C_Invoice_ID = ci.C_Invoice_ID

INNER JOIN uns_vatdocno uvt ON uvt.uns_vatdocno_id = uv.uns_vatdocno_id

INNER JOIN uns_vatregisteredsequences uvr ON uvr.uns_vatregisteredsequences_id = uvt.uns_vatregisteredsequences_id

INNER JOIN ad_org ao ON ao.ad_org_id = ci.ad_org_id

LEFT JOIN ad_orginfo aoi ON aoi.ad_org_id = ao.ad_org_id

LEFT JOIN c_location cl ON cl.c_location_id = aoi.c_location_id

INNER JOIN c_bpartner cb ON cb.c_bpartner_id = ci.c_bpartner_id

INNER JOIN c_bpartner_location cbl ON cbl.c_bpartner_location_id = ci.c_bpartner_location_id

INNER JOIN c_location clb ON clb.c_location_id = cbl.c_location_id

INNER JOIN c_invoiceline cil ON cil.c_invoice_id = ci.c_invoice_id

INNER JOIN m_product mp ON mp.m_product_id = cil.m_product_id

INNER JOIN c_uom cu ON cu.c_uom_id = cil.c_uom_id

WHERE uv.UNS_VATDocInOut_ID = $P{UNS_VATDocInOut_ID}]]>
	</queryString>

	<parameter name="UNS_Charge_RS_ID" class="java.lang.Integer"/>
	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select org.name as org,req.name as nmtrx,req.documentno as documentno,dt.docbasetype as docbasetype,req.datetrx as datedoc,bp.name as requestuser,cd.description as description,coalesce(cd.amountconfirmed, 0) as confirmedamt,
(select coalesce(amountconfirmed, 0) from uns_charge_detail where uns_charge_detail_id = cd.reference_id) as referenceamt,confirmation.updatedby,
(select name from ad_user where ad_user_id = $P{AD_User_ID}  ) as admin,
ba.name as bankaccount
from uns_charge_rs req
inner join c_bpartner bp on bp.c_bpartner_id = req.c_bpartner_id
inner join uns_charge_detail cd on cd.uns_charge_rs_id = req.uns_charge_rs_id
inner join uns_charge_confirmation confirmation on confirmation.uns_charge_rs_id = req.uns_charge_rs_id
left join c_bankstatement bs on bs.c_bankstatement_id = confirmation.c_bankstatement_id
left join c_bankaccount ba on ba.c_bankaccount_id = bs.c_bankaccount_id
inner join c_doctype dt on dt.c_doctype_id = req.c_doctype_id
left join ad_org org on org.ad_org_id=req.ad_org_id
where req.uns_charge_rs_id =$P{UNS_Charge_RS_ID}  and cd.chargetype='CS' and  cd.isactive = 'Y']]>
	</queryString>

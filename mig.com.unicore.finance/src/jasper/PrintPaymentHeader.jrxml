	<queryString>
		<![CDATA[SELECT
 bp.name as vendor,coalesce(sl.name,'') as nm_sales,
 coalesce(p.documentno,'') as documentno,
 p.datetrx as datetrx,case when p.c_charge_id!=0 then (select name from c_charge where c_charge_id=p.c_charge_id) else '' end as mncharge,
 coalesce(p.description,' ') as description,org.name as nmorg,
 coalesce(i.description,' ') as idescp,bc.name as nmbank,
 coalesce(i.documentno,'') as referenceno,
 i.dateinvoiced as dateinvoiced,
 i.dateinvoiced + INTERVAL '1 DAYS' * pt.netdays as duedate,
 i.grandtotal as grandtotal,
 p.payamt as invoiceopen,
 p.totalamt as payamt,
 p.description as allocatedescription,
now() as tanggal,u.name as Nmuser
FROM
 c_payment p
left JOIN  c_invoice i ON i.c_invoice_id = p.c_invoice_id
left JOIN  c_paymentterm pt on pt.c_paymentterm_id = i.c_paymentterm_id
left JOIN  c_bpartner bp ON bp.c_bpartner_id = p.c_bpartner_id
left join  c_bankaccount bc on bc.c_bankaccount_id=p.c_bankaccount_id
left join ad_org org on org.ad_org_id=p.ad_org_id
left join ad_user sl on sl.ad_user_id=i.salesrep_id
LEFT join ad_user u on u.ad_user_id = $P{AD_User_ID}
WHERE p.c_payment_id =$P{C_Payment_ID}]]>
	</queryString>

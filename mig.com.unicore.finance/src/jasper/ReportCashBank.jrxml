	<queryString>
		<![CDATA[SELECT cb.name AS Bank, cba.name AS NamaAkun, cba.accountno AS NoAkun, arl.name AS TipeAkun, cba.currentbalance AS Saldo, cc.iso_code AS MataUang, ucb.uns_chequebook_ID, ucb.name AS NameCek

FROM c_bank cb

INNER JOIN c_bankaccount cba ON cba.c_bank_id = cb.c_bank_id

INNER JOIN c_currency cc ON cc.c_currency_id = cba.c_currency_id

INNER JOIN ad_ref_list arl ON arl.value = cba.bankaccounttype

LEFT JOIN uns_chequebook ucb ON ucb.c_bankaccount_id = cba.c_bankaccount_id

WHERE arl.ad_reference_id = 216

ORDER BY cb.name, cba.name, cba.accountno]]>
	</queryString>

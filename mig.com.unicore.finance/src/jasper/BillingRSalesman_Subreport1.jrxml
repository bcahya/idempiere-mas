	<queryString>
		<![CDATA[select now() as tglskrg,u.name as user,data.tanggal as tanggal,og.name as org,data.nmsales,sum(data.tbayar+data.bgiro) as tbayar,sum(data.directdp) as directdp,
sum(data.credit) as credit,sum(data.directdebit) as directdebit,sum(data.tcheck) as tcheck,sum(data.tacount) as tacount,sum(data.tcash) as tcash,sum(data.bgiro) as bgiro
from(
select cp.datetrx as tanggal,org.ad_org_id as org,u.name as nmsales,(cp.payamt) as tbayar,
(case when cp.tendertype='A' then (cl.paytooverunderamount) else 0 end) as directdp,(case when cp.tendertype='C' then (cl.paytooverunderamount) else 0 end) as credit,
(case when cp.tendertype='D' then (cl.paytooverunderamount) else 0 end) as directdebit,(case when cp.tendertype='K' then (cl.paytooverunderamount) else 0 end) as tcheck,
(case when cp.tendertype='T' then (cl.paytooverunderamount) else 0 end) as tacount,(case when cp.tendertype='X' then (cl.paytooverunderamount) else 0 end) as tcash,0 as bgiro
from c_payment as cp
LEFT JOIN c_bankaccount ba on ba.c_bankaccount_id=cp.c_bankaccount_id
left join ad_org org on org.ad_org_id=cp.ad_org_id
left join C_PaymentAllocate cl on cl.c_payment_id=cp.c_payment_id
inner join c_invoice ci on ci.c_invoice_id=cl.c_invoice_id
left join ad_user u on u.ad_user_id=ci.salesrep_id
where cp.uns_pr_allocation_id is null and cp.docstatus in('CO','CL')
and cp.c_doctype_id in(select c_doctype_id from c_doctype where docbasetype='ARR')
and cp.datetrx between $P{StartDate} and $P{EndDate}
union all
select cp.datetrx as tanggal,org.ad_org_id as org,u.name as nmsales,(cp.payamt) as tbayar,
(case when cp.tendertype='A' then (cp.payamt) else 0 end) as directdp,(case when cp.tendertype='C' then (cp.payamt) else 0 end) as credit,
(case when cp.tendertype='D' then (cp.payamt) else 0 end) as directdebit,(case when cp.tendertype='K' then (cp.payamt) else 0 end) as tcheck,
(case when cp.tendertype='T' then (cp.payamt) else 0 end) as tacount,(case when cp.tendertype='X' then (cp.payamt) else 0 end) as tcash,0 as bgiro
from c_payment as cp
left join ad_org org on org.ad_org_id=cp.ad_org_id
left join uns_pr_allocation pa on pa.uns_pr_allocation_id=cp.uns_pr_allocation_id
left join uns_billinggroup_result bl on bl.uns_billinggroup_result_id=pa.uns_billinggroup_result_id
left join UNS_BillingGroup bg on bg.uns_billinggroup_id=bl.uns_billinggroup_id
left join ad_user u on u.ad_user_id=bg.salesrep_id
where cp.uns_pr_allocation_id is not null and cp.docstatus in('CO','CL')
and cp.c_doctype_id in(select c_doctype_id from c_doctype where docbasetype='ARR')
and cp.datetrx between $P{StartDate} and $P{EndDate}
union all
select cb.receiptdate as tanggal,cb.ad_org_id as org,u.name as nmsales,0 as tbayar,0 as directdp,0 as credit,0 as directdebit,0 as tcheck,0 as tacount, 0 as tcash,il.paidamt as bgiro
from uns_customerbg cb
left join uns_customerbg_action cba on cba.uns_customerbg_id=cb.uns_customerbg_id
left join UNS_CustomerBG_InvList il on il.uns_customerbg_id=cb.uns_customerbg_id
left join c_invoice ci on ci.c_invoice_id=il.c_invoice_id
left join ad_user u on u.ad_user_id=ci.salesrep_id
where il.c_invoice_id is not null
and cb.receiptdate between $P{StartDate} and $P{EndDate}
union all
select cp.datetrx as tanggal,org.ad_org_id as org,cb.name as nmsales,(cp.payamt) as tbayar,
(case when cp.tendertype='A' then (cp.payamt) else 0 end) as directdp,(case when cp.tendertype='C' then (cp.payamt) else 0 end) as credit,
(case when cp.tendertype='D' then (cp.payamt) else 0 end) as directdebit,(case when cp.tendertype='K' then (cp.payamt) else 0 end) as tcheck,
(case when cp.tendertype='T' then (cp.payamt) else 0 end) as tacount,(case when cp.tendertype='X' then (cp.payamt) else 0 end) as tcash,0 as bgiro
from c_payment as cp
left join ad_org org on org.ad_org_id=cp.ad_org_id
left join c_bpartner cb on cb.c_bpartner_id=cp.c_bpartner_id
where cp.c_payment_id not in(select c_payment_id from c_paymentallocate)  and cp.docstatus in('CO','CL')
and cp.c_doctype_id in(select c_doctype_id from c_doctype where docbasetype='ARR')
and cp.datetrx between $P{StartDate} and $P{EndDate}
union all
select cb.receiptdate as tanggal,cb.ad_org_id as org,u.name as nmsales,0 as tbayar,0 as directdp,0 as credit,0 as directdebit,0 as tcheck,0 as tacount, 0 as tcash,il.paidamt as bgiro
from uns_customerbg cb
left join uns_customerbg_action cba on cba.uns_customerbg_id=cb.uns_customerbg_id
left join UNS_CustomerBG_InvList il on il.uns_customerbg_id=cb.uns_customerbg_id
left join c_order ci on ci.c_order_id=il.c_order_id
left join ad_user u on u.ad_user_id=ci.salesrep_id
where il.c_order_id is not null and cb.receiptdate between $P{StartDate} and $P{EndDate} ) as data
left join ad_org og on og.ad_org_id=data.org
left join ad_user u on u.ad_user_id=$P{AD_User_ID}
where data.org in(select ad_org_id from ad_org where c_salesregion_id=$P{C_SalesRegion_ID})
group by now(),u.name,data.tanggal,og.name,data.nmsales
order by og.name,data.nmsales,data.tanggal]]>
	</queryString>

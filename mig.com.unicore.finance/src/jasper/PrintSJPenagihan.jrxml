	<subDataset name="Table Dataset 1" uuid="80501013-82f7-45df-9bf1-9420795fcccd"/>
	<parameter name="UNS_GroupingBilling_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT org.Name AS Org, invoiceopen(ivc."c_invoice_id",null) as invoiceopen,uns_billinggroup."documentno" AS documentno,
now() AS  tanggalskrg,uns_rayon."name" AS rayon,uns_billinggroup."datedoc" AS tanggal,     uns_billingline."duedate" AS duedate,ivc."documentno" AS nofaktur,lc."bpartnername" AS namacustomer,     uns_billingline."dateinvoiced" AS tglfak,ivc."grandtotal" AS jmlfak,ad_user."name" AS sales,lc."phone" AS notlp,

(SELECT COALESCE(SUM(pa.amount),0) FROM C_PaymentAllocate pa WHERE pa.C_Invoice_ID=ivc.C_Invoice_ID AND EXISTS (SELECT 1 FROM C_PaymentAllocate al WHERE al.C_Payment_ID = pa.C_Payment_ID and pa.C_PaymentAllocate_ID <> al.C_PaymentAllocate_ID AND EXISTS (SELECT 1 FROM C_Invoice iv WHERE iv.C_Invoice_ID = al.C_Invoice_ID AND EXISTS (SELECT 1 FROM C_DocType dt WHERE dt.C_DocType_ID = iv.C_DocType_ID and dt.DocBaseType = 'ARC'))) AND EXISTS (SELECT 1 FROM C_Payment p WHERE p.C_Payment_ID = pa.C_Payment_ID AND p.DocStatus IN ('CO', 'CL'))) AS retur,

COALESCE((SELECT SUM(paidAMt) FROM UNS_CustomerBG_invlist where c_invoice_id=ivc."c_invoice_id" and uns_customerbg_id in(select uns_customerbg_id from uns_customerbg where disbursementdate is null)),0) as giro
FROM
     "adempiere"."uns_rayon" uns_rayon INNER JOIN "uns_billinggroup" uns_billinggroup ON uns_rayon."uns_rayon_id" = uns_billinggroup."uns_rayon_id"
     INNER JOIN "adempiere"."uns_billing" uns_billing ON uns_billinggroup."uns_billinggroup_id" = uns_billing."uns_billinggroup_id"
     INNER JOIN "adempiere"."ad_user" ad_user ON uns_billinggroup."salesrep_id" = ad_user."ad_user_id"
     INNER JOIN "adempiere"."uns_billingline" uns_billingline ON uns_billing."uns_billing_id" = uns_billingline."uns_billing_id"
 INNER JOIN c_invoice ivc on uns_billingline.c_invoice_id = ivc.c_invoice_id
     inner join c_bpartner_location lc ON lc.c_bpartner_location_id = ivc.c_bpartner_location_id
INNER JOIN AD_OrgInfo oi ON oi.AD_Org_ID = ivc.AD_Org_ID
INNER JOIN AD_Org org ON org.AD_Org_ID = oi.Parent_Org_ID
WHERE
     uns_billinggroup."uns_billinggroup_id"= $P{UNS_GroupingBilling_ID}
order by org.Name, lc."bpartnername"]]>
	</queryString>

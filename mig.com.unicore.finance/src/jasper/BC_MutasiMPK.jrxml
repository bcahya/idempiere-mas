	<parameter name="DateFrom" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="DateTo" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT p.value, p.name, uom.UOMSymbol, c.name as perusahaan, ppc.value,
CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate<$P{DateFrom} AND mt.M_Product_ID=p.M_Product_ID) is null THEN 0
else
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate<$P{DateFrom} AND mt.M_Product_ID=p.M_Product_ID)
END as laststock,
CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V+', 'C+', 'W+', 'P+') AND mt.M_Product_ID=p.M_Product_ID) is null THEN 0
else
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V+', 'C+', 'W+', 'P+') AND mt.M_Product_ID=p.M_Product_ID)
END AS pemasukan,
CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V-', 'C-', 'W-', 'P-', 'I-', 'S-') AND mt.M_Product_ID=p.M_Product_ID) is null THEN  0
ELSE
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V-', 'C-', 'W-', 'P-', 'I-', 'S-') AND mt.M_Product_ID=p.M_Product_ID)
END AS pengeluaran,
CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('I+') AND mt.M_Product_ID=p.M_Product_ID) is null then 0
ELSE
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('I+') AND mt.M_Product_ID=p.M_Product_ID)
END AS penyesuaian,
CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate=$P{DateTo}
AND mt.MovementType IN ('I+') AND mt.M_Product_ID=p.M_Product_ID) is null then 0
ELSE
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate=$P{DateTo}
AND mt.MovementType IN ('I+') AND mt.M_Product_ID=p.M_Product_ID)
END AS stockopname,
SUM(soh1.qtyonHand) as currentstock,
SUM(soh1.qtyonHand) - (CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate<$P{DateFrom} AND mt.M_Product_ID=p.M_Product_ID) is null THEN 0
else
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate<$P{DateFrom} AND mt.M_Product_ID=p.M_Product_ID)
END + CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V+', 'C+', 'W+', 'P+') AND mt.M_Product_ID=p.M_Product_ID) is null THEN 0
else
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V+', 'C+', 'W+', 'P+') AND mt.M_Product_ID=p.M_Product_ID)
END - CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V-', 'C-', 'W-', 'P-', 'I-', 'S-') AND mt.M_Product_ID=p.M_Product_ID) is null THEN  0
ELSE
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('V-', 'C-', 'W-', 'P-', 'I-', 'S-') AND mt.M_Product_ID=p.M_Product_ID)
END + CASE WHEN
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('I+') AND mt.M_Product_ID=p.M_Product_ID) is null then 0
ELSE
(SELECT SUM(mt.MovementQty) FROM M_Transaction mt WHERE mt.MovementDate BETWEEN $P{DateFrom} AND $P{DateTo}
AND mt.MovementType IN ('I+') AND mt.M_Product_ID=p.M_Product_ID)
END) as selisih
FROM M_Product p INNER JOIN M_StorageOnHand soh1 ON soh1.M_Product_ID=p.M_Product_ID
INNER JOIN M_Product_Category pc ON p.M_Product_Category_ID=pc.M_Product_Category_ID
INNER JOIN M_Product_Category spc ON spc.M_Product_Category_ID=pc.M_Product_Category_Parent_ID
LEFT JOIN M_Product_Category ppc ON ppc.M_Product_Category_ID=spc.M_Product_Category_Parent_ID
INNER JOIN AD_Client c ON p.AD_Client_ID=c.AD_Client_ID
INNER JOIN C_UOM uom ON p.C_UOM_ID=uom.C_UOM_ID
WHERE pc.value in ('P-EQM', 'O-CEQ', 'O-CUA', 'O-EQM', 'O-STY', 'O-TEQ')
GROUP BY p.M_Product_ID, c.AD_Client_ID, uom.C_UOM_ID, ppc.value
ORDER BY ppc.value DESC]]>
	</queryString>

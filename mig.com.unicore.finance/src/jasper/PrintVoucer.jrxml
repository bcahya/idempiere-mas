	<parameter name="AD_User_ID" class="java.lang.Integer"/>
	<parameter name="C_Payment_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select now() as tglskrg,u.name as nmcetak,ar.noref,ar.nmbank,ar.docno,ar.tgltrx,ar.ket,ar.debit,ar.kredit
from (
select cp.reference_id as noref,bc.name as nmbank,cp.documentno as docno,cp.datetrx as tgltrx,cp.description as ket,
case when cp.c_doctype_id=1000008 then cp.totalamt else 0 end as debit,
case when cp.c_doctype_id=1000009 then cp.totalamt else 0 end as kredit
from c_payment cp
left join c_charge crg on crg.c_charge_id=cp.c_charge_id
left join c_bankaccount bc on bc.c_bankaccount_id=cp.c_bankaccount_id
where cp.reference_id is not null and cp.docstatus in('CO','DR','IP')
and cp.c_payment_id=$P{C_Payment_ID}
union all
select p.reference_id as noref,bc.name as nmbank,p.documentno as docno,p.datetrx as tgltrx,p.description as ket,
case when p.c_doctype_id=1000008 then p.totalamt else 0 end as debit,
case when p.c_doctype_id=1000009 then p.totalamt else 0 end as kredit
from (
select cp.reference_id as noref
from c_payment cp
left join c_charge crg on crg.c_charge_id=cp.c_charge_id
where cp.reference_id is not null and cp.docstatus in('CO','DR','IP')
and cp.c_payment_id=$P{C_Payment_ID}) as ar
left join c_payment p on p.c_payment_id=ar.noref
left join c_bankaccount bc on bc.c_bankaccount_id=p.c_bankaccount_id
) as ar
left join ad_user u on u.ad_user_id=$P{AD_User_ID}]]>
	</queryString>

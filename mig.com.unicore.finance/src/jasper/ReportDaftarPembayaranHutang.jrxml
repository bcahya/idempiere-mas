	<queryString>
		<![CDATA[SELECT
 bp.name as vendor,
 p.documentno as documentno,
 p.datetrx as datetrx,
 coalesce(p.description,' ') as description,
 coalesce(i.description,' ') as idescp,
 COALESCE(i.documentno, co.DocumentNo) as referenceno,
 COALESCE(i.dateinvoiced, co.DateOrdered) as dateinvoiced,bc.name,
 COALESCE(i.dateinvoiced, co.DateOrdered) + INTERVAL '1 DAYS' * pt.netdays as duedate,
 COALESCE(i.grandtotal, co.GrandTotal) as grandtotal,
 pa.invoiceamt as invoiceopen,
 pa.paytooverunderamount as payamt,
 pa.description as allocatedescription,
now() as tanggal,u.name as Nmuser,
CASE WHEN pa.C_Invoice_ID IS NOT NULL THEN 'Y' ELSE 'N' END AS isInvoice,
mi.documentno as nom,mr.documentno as mno,coalesce(co.documentno,'') as nopo,coalesce(com.documentno,'') as mnopo,
coalesce(case when p.c_charge_id!=0 then (select name from c_charge where c_charge_id=p.c_charge_id) else '' end,'') as mncharge,
case when p.c_charge_id!=0 and  p.C_DocType_ID=1000009 then p.Chargeamt*-1 else 0 end as rpcharge
FROM
 c_paymentallocate pa
LEFT JOIN  c_invoice i ON i.c_invoice_id = pa.c_invoice_id
LEFT JOIN C_Order co ON co.C_Order_ID = pa.C_Order_ID
INNER JOIN  c_paymentterm pt on pt.c_paymentterm_id = COALESCE(i.c_paymentterm_id, co.C_PaymentTerm_ID)
INNER JOIN  c_payment p ON p.c_payment_id = pa.c_payment_id
INNER JOIN  c_bpartner bp ON bp.c_bpartner_id = p.c_bpartner_id
INNER JOIN c_bankaccount bc on bc.c_bankaccount_id=p.c_bankaccount_id
LEFT JOIN m_inout mi on mi.c_order_id=co.c_order_id
LEFT JOIN m_inout mr on mr.c_invoice_id=i.c_invoice_id
left join c_order com on com.c_order_id=i.c_order_id
left JOIN ad_user u on u.ad_user_id = $P{AD_User_ID}
WHERE p.C_Payment_ID = $P{C_Payment_ID}
and p.c_doctype_id in(select c_doctype_id from c_doctype  where docbasetype='APP')]]>
	</queryString>
